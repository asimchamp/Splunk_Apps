<form refresh="180" stylesheet="custom.css">
  <label>Search By Attack Name</label>
  <fieldset submitButton="true" autoRun="true">
    <input type="time" token="field1">
      <label>Time Range</label>
      <default>
        <earliest>-7d@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="name" searchWhenChanged="true">
      <label>Attack Type</label>
      <default>*</default>
      <choice value="*">All</choice>
      <search>
        <query>sourcetype=incapsula | dedup CEF_Name | table CEF_Name</query>
        <earliest>$field1.earliest$</earliest>
        <latest>$field1.latest$</latest>
      </search>
      <fieldForLabel>CEF_Name</fieldForLabel>
      <fieldForValue>CEF_Name</fieldForValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <single>
        <title>Number Of Events</title>
        <search>
          <query>sourcetype=incapsula CEF_Name="$name$"  | stats count(CEF_Vendor)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="refresh.auto.interval">30</option>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of Attackers</title>
        <search>
          <query>sourcetype=incapsula CEF_Name="$name$" | stats dc(src)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="refresh.auto.interval">30</option>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of Site ID's</title>
        <search>
          <query>sourcetype=incapsula CEF_Name="$name$" | stats dc(siteid)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="refresh.auto.interval">30</option>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of Attack Types</title>
        <search>
          <query>index="incapsula" CEF_Name="$name$" | stats dc(CEF_Name)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="refresh.auto.interval">30</option>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Top 10 Event Types</title>
