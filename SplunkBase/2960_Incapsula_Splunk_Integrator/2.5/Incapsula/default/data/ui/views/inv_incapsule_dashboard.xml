<form>
  <label>INV Incapsule Dashboard - By Attacker Address</label>
  <fieldset submitButton="true" autoRun="true">
    <input type="text" token="src">
      <label>Name</label>
      <default>*</default>
    </input>
    <input type="time" token="field1">
      <label></label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <single>
        <title>Number Of Events</title>
        <search>
          <query>sourcetype=incapsula src=$src$ | stats count(CEF_Vendor)</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of IP's</title>
        <search>
          <query>sourcetype="incapsula" src=$src$ | stats dc(src)</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Top 10 Event Types</title>
