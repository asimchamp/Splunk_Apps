<form refresh="180" stylesheet="custom.css">
  <label>Free Search</label>
  <fieldset submitButton="true" autoRun="true">
    <input type="text" token="free">
      <label>Free Text</label>
      <default>*</default>
    </input>
    <input type="time" token="field1" searchWhenChanged="true">
      <label>Time Range</label>
      <default>
        <earliest>-7d@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <single>
        <title>Number Of Attacks</title>
        <search>
          <query>index=incapsula "*$free$*" | stats count(CEF_Vendor)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of Attackers</title>
        <search>
          <query>index=incapsula "*$free$*" | stats dc(src)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of Site ID's</title>
        <search>
          <query>index=incapsula "*$free$*" | stats dc(suid)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Number Of Attack Types</title>
        <search>
          <query>index="incapsula" "*$free$*" | stats dc(CEF_Name)</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="linkView">search</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Top 10 Attack Types</title>
