<view template="dashboard.html" isSticky="True">
	<label>All Console Users</label>
	<module name="AccountBar" layoutPanel="appHeader"/>
	<module name="AppBar" layoutPanel="navigationHeader"/>
	<module name="Message" layoutPanel="messaging">
		<param name="filter">*</param>
		<param name="clearOnJobDispatch">False</param>
		<param name="maxSize">1</param>
		<param name="level">warn</param>
	</module>
	<module name="TitleBar" layoutPanel="viewHeader">
		<param name="actionsMenuFilter">dashboard</param>
	</module>
	<module name="TimeRangePicker" layoutPanel="viewHeader" autoRun="true">
		<param name="default">Last 24 hours</param>
		<param name="searchWhenChanged">True</param>
		<module name="HiddenSearch" layoutPanel="panel_row2_col1" group="Events">
			<param name="search">| savedsearch "All Console Users"</param>
			<param name="maxCount">10000000</param>
			<module name="JobProgressIndicator">
				<module name="HiddenPostProcess" layoutPanel="panel_row2_col1" group="Events">
					<param name="search">| timechart bins=50 minspan=15m sum(count) by EventSubType limit=0</param>
					<module name="HiddenChartFormatter">
						<param name="charting.chart">column</param>
						<param name="charting.legend.placement">right</param>
						<param name="charting.axisTitleX.text"></param>
						<param name="charting.axisTitleX.visibility">collapsed</param>
						<param name="charting.chart.stackMode">stacked</param>
						<param name="charting.data.count">1000000</param>
						<module name="JSChart">
							<module name="HiddenSearch">
								<param name="search">eventtype=bit9_event [search eventtype=bit9_event EventSubType="Console user login" | fields UserName]</param>
								<module name="ConvertToIntention">
									<param name="intention">
										<param name="name">time</param>
									</param>
									<param name="intention">
										<param name="name">addterm</param>
										<param name="arg">
											<param name="EventSubType">$click.name2$</param>
										</param>
									</param>
									<module name="ViewRedirector">
										<param name="viewTarget">flashtimeline</param>
									</module>
								</module>
							</module>
						</module>
					</module>
				</module>
				<module name="HiddenPostProcess" layoutPanel="panel_row3_col1" group="Policy Management">
					<param name="search">| search EventType="Policy Management" | timechart bins=50 minspan=15m sum(count) by UserName limit=0</param>
					<module name="HiddenChartFormatter">
						<param name="charting.chart">column</param>
						<param name="charting.legend.placement">right</param>
						<param name="charting.legend.labelStyle.overflowMode">none</param>
						<param name="charting.axisTitleX.text"></param>
						<param name="charting.axisTitleX.visibility">collapsed</param>
						<param name="charting.axisTitleY.text"></param>
						<param name="charting.axisTitleY.visibility">collapsed</param>
						<param name="charting.chart.stackMode">stacked</param>
						<param name="charting.data.count">50000</param>
						<module name="JSChart">
							<module name="HiddenSearch">
								<param name="search">eventtype=bit9_event EventType="Policy Management"</param>
								<module name="ConvertToIntention">
									<param name="intention">
										<param name="name">time</param>
									</param>
									<param name="intention">
										<param name="name">addterm</param>
										<param name="arg">
											<param name="UserName">$click.name2$</param>
										</param>
									</param>
									<module name="ViewRedirector">
										<param name="viewTarget">flashtimeline</param>
									</module>
								</module>
							</module>
						</module>
					</module>
				</module>
				<module name="HiddenPostProcess" layoutPanel="panel_row3_col2" group="Computer Management">
					<param name="search">| search EventType="Computer Management" | timechart bins=50 minspan=15m sum(count) by UserName limit=0</param>
					<module name="HiddenChartFormatter">
						<param name="charting.chart">column</param>
						<param name="charting.legend.placement">right</param>
						<param name="charting.legend.labelStyle.overflowMode">none</param>
						<param name="charting.axisTitleX.text"></param>
						<param name="charting.axisTitleX.visibility">collapsed</param>
						<param name="charting.axisTitleY.text"></param>
						<param name="charting.axisTitleY.visibility">collapsed</param>
						<param name="charting.chart.stackMode">stacked</param>
						<param name="charting.data.count">50000</param>
						<module name="JSChart">
							<module name="HiddenSearch">
								<param name="search">eventtype=bit9_event EventType="Computer Management"</param>
								<module name="ConvertToIntention">
									<param name="intention">
										<param name="name">time</param>
									</param>
									<param name="intention">
										<param name="name">addterm</param>
										<param name="arg">
											<param name="UserName">$click.name2$</param>
										</param>
									</param>
									<module name="ViewRedirector">
										<param name="viewTarget">flashtimeline</param>
									</module>
								</module>
							</module>
						</module>
					</module>
				</module>
				<module name="HiddenPostProcess" layoutPanel="panel_row3_col3" group="Session &amp; General Management">
					<param name="search">| search (EventType="Session Management" OR EventType="General Management") | timechart bins=50 minspan=15m sum(count) by UserName limit=0</param>
					<module name="HiddenChartFormatter">
						<param name="charting.chart">column</param>
						<param name="charting.legend.placement">right</param>
						<param name="charting.legend.labelStyle.overflowMode">none</param>
						<param name="charting.axisTitleX.text"></param>
						<param name="charting.axisTitleX.visibility">collapsed</param>
						<param name="charting.axisTitleY.text"></param>
						<param name="charting.axisTitleY.visibility">collapsed</param>
						<param name="charting.chart.stackMode">stacked</param>
						<param name="charting.data.count">50000</param>
						<module name="JSChart">
							<module name="HiddenSearch">
								<param name="search">eventtype=bit9_event (EventType="Session Management" OR EventType="General Management")</param>
								<module name="ConvertToIntention">
									<param name="intention">
										<param name="name">time</param>
									</param>
									<param name="intention">
										<param name="name">addterm</param>
										<param name="arg">
											<param name="UserName">$click.name2$</param>
										</param>
									</param>
									<module name="ViewRedirector">
										<param name="viewTarget">flashtimeline</param>
									</module>
								</module>
							</module>
						</module>
					</module>
				</module>
				<module name="HiddenPostProcess" layoutPanel="panel_row4_col1" group="Top Ten Users - Global Approvals">
					<param name="search">| search EventSubTypeId=627 | stats sum(count) as count by UserName | sort -count | head 10</param>
					<module name="HiddenChartFormatter">
						<param name="charting.chart">bar</param>
						<param name="charting.legend.placement">none</param>
						<param name="charting.legend.labelStyle.overflowMode">none</param>
						<param name="charting.axisTitleX.text"></param>
						<param name="charting.axisTitleX.visibility">collapsed</param>
						<param name="charting.axisTitleY.text"></param>
						<param name="charting.axisTitleY.visibility">collapsed</param>
						<param name="charting.chart.stackMode">none</param>
						<module name="JSChart">
							<module name="HiddenSearch">
								<param name="search">eventtype=bit9_event EventSubTypeId=627</param>
								<module name="ConvertToIntention">
									<param name="intention">
										<param name="name">addterm</param>
										<param name="arg">
											<param name="UserName">$click.value$</param>
										</param>
									</param>
									<module name="ViewRedirector">
										<param name="viewTarget">flashtimeline</param>
									</module>
								</module>
							</module>
						</module>
					</module>
				</module>
				<module name="HiddenPostProcess" layoutPanel="panel_row4_col2" group="Top Ten Users - Local Approvals">
					<param name="search">| search EventSubTypeId=623 | stats sum(count) as count by UserName | sort -count | head 10</param>
					<module name="HiddenChartFormatter">
						<param name="charting.chart">bar</param>
						<param name="charting.legend.placement">none</param>
						<param name="charting.legend.labelStyle.overflowMode">none</param>
						<param name="charting.axisTitleX.text"></param>
						<param name="charting.axisTitleX.visibility">collapsed</param>
						<param name="charting.axisTitleY.text"></param>
						<param name="charting.axisTitleY.visibility">collapsed</param>
						<param name="charting.chart.stackMode">none</param>
						<module name="JSChart">
							<module name="HiddenSearch">
								<param name="search">eventtype=bit9_event EventSubTypeId=623</param>
								<module name="ConvertToIntention">
									<param name="intention">
										<param name="name">addterm</param>
										<param name="arg">
											<param name="UserName">$click.value$</param>
										</param>
									</param>
									<module name="ViewRedirector">
										<param name="viewTarget">flashtimeline</param>
									</module>
								</module>
							</module>
						</module>
					</module>
				</module>
			</module>
		</module>
	</module>
</view>
