
<view template="dashboard.html" onunloadCancelJobs="True" autoCancelInterval="100">
  <!--  autoCancelInterval is set here to 100  -->
  <label>F5 - APM - Sessions variables by session id - Report</label>
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="navigationHeader">
    <param name="showActionsMenu">False</param>
  </module>

  <module name="SearchBar" layoutPanel="mainSearchControls">
    <param name="useAssistant">true</param>
    <param name="useTypeahead">true</param>
    <module name="TimeRangePicker">
      <param name="default">-4h</param>
      <module name="ViewRedirector">
        <param name="viewTarget">search</param>
      </module>
    </module>
  </module>

  <module name="TimeRangePicker" layoutPanel="panel_row1_col1" group="Users sessions">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>

	
	<module name="HiddenSearch" autoRun="False">
	  <param name="search"><![CDATA[sourcetype="apm_log" |dedup session_id| sort - _time | table _time, session_id ]]></param>
	  <module name="SubmitButton">
	    <module name="JobProgressIndicator"/>
	    <param name="label">Search</param>
            <module name="Paginator" layoutPanel="panel_row1_col1">
              <param name="entityName">results</param>
              <param name="count">10</param>
              <param name="maxPages">10</param>
	      <module name="SimpleResultsTable">
	      </module>
            </module>
	  </module>
	  <module name="SearchTextSetting" layoutPanel="panel_row1_col2" autoRun="False">
	    <param name="label">Enter Session Id</param>
	    <param name="elementName">session_id</param>
	    <param name="settingName">s_id</param>
	    <module name="ConvertToIntention">
	      <param name="settingToConvert">s_id</param>
	      <param name="intention">
		<param name="name">stringreplace</param>
		<param name="arg">
		  <param name="s_id">
		    <param name="fillOnEmpty">True</param>
		    <param name="value">$target$</param>
		  </param>
		</param>
	      </param>
	      
	      <module name="HiddenSearch" autoRun="False">
		<param name="search"><![CDATA[sourcetype="apm_log" session.* session_id = "$s_id$" | rex field=_raw "(?<sess_var>session\..*) (?<act>is) (?<sess_var_val>.*\Z)"| rex "Session variable '(?<sess_var>.*)' (?<act>set to) '(?<sess_var_val>.*)[?:'|]$"| table sess_var, act, sess_var_val]]></param>
		<module name="SubmitButton">
		  <module name="JobProgressIndicator"/>
		  <param name="label">Search</param>
		  <module name="SimpleResultsHeader">
		    <param name="entityName">results</param>
		    <param name="headerFormat">Events for session - $s_id$</param>
		  </module>
                  <module name="Paginator" layoutPanel="panel_row1_col2" label="Policy steps">
                    <param name="entityName">results</param>
                    <param name="count">50</param>
                    <param name="maxPages">100</param>
		    <module name="SimpleResultsTable">
		    </module>
                  </module>
		</module>
	      </module>
	    </module>
	  </module>
	  
	</module>
    
  </module>
  
</view>

