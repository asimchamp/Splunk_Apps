<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Secret Server Dashboard</label>
  <row>
    <chart>
      <searchName>Top Users</searchName>
      <title>Top Users in the Last 30 Days</title>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.showMarkers">false</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.secondaryAxis.maximumNumber"/>
      <option name="charting.secondaryAxis.minimumNumber"/>
      <option name="charting.secondaryAxis.scale"/>
      <option name="charting.secondaryAxisTitle.text">Frequency</option>
    </chart>
    <chart>
      <searchName>Heartbeat Failures in the last 24 Hours</searchName>
      <title>Heartbeat Failures</title>
      <option name="charting.axisTitleY.text">## of failures</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.showMarkers">false</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.primaryAxisTitle.text">time</option>
      <option name="charting.secondaryAxis.maximumNumber"/>
      <option name="charting.secondaryAxis.minimumNumber"/>
      <option name="charting.secondaryAxis.scale"/>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
    <!-- Static content, remove upon editing-->
    <!-- End of static content -->
  </row>
  <row>
    <table>
      <searchName>User Views</searchName>
      <title>Secret Views in last 48 Hours</title>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <event>
      <searchName>Thycotic Events</searchName>
      <title>Secret Server Events</title>
      <earliestTime></earliestTime>
      <latestTime></latestTime>
      <option name="count">10</option>
      <option name="displayRowNumbers">False</option>
      <option name="maxLines">10</option>
      <option name="segmentation">outer</option>
      <option name="softWrap">true</option>
    </event>
  </row>
  <!-- First row of a single comment -->
  <!-- Second row of 2 panels -->
</dashboard>
