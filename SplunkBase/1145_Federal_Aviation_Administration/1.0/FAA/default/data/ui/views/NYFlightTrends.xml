<view autoCancelInterval="90" isVisible="true" objectMode="SimpleDashboard" onunloadCancelJobs="true" refresh="-1" template="dashboard.html">
  <label>New York Flight Trends</label>
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="DashboardTitleBar" layoutPanel="viewHeader"/>
  <module name="Message" layoutPanel="navigationHeader">
    <param name="level">warn</param>
    <param name="filter">splunk.search.job</param>
    <param name="clearOnJobDispatch">True</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row1_col1">
    <param name="searchWhenChanged">True</param>
    <param name="default">All Time</param>
  <module name="HiddenSearch" layoutPanel="panel_row2_col1" group="# Flights From NY" autoRun="True">
    <param name="search">sourcetype="airline_csv" OriginCityName="New York*" | stats count | rangemap field=count low=0-20000 elevated=20001-30000 default=severe</param>
    <param name="groupLabel"># Flights From NY</param>
    <module name="ViewstateAdapter">
      <param name="suppressionList">
        <item>displayRowNumbers</item>
        <item>classField</item>
        <item>count</item>
        <item>afterLabel</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="SingleValue">
            <param name="classField">range</param>
            <param name="afterLabel"> flights</param>
            <module name="Gimp"/>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="HiddenSearch" layoutPanel="panel_row2_col2" group="# Flights To NY" autoRun="True">
    <param name="search">sourcetype="airline_csv" DestCityName="New York*" | stats count | rangemap field=count low=0-20000 elevated=20001-30000 default=severe</param>
    <param name="earliest">0</param>
    <param name="groupLabel"># Flights To NY</param>
    <module name="ViewstateAdapter">
      <param name="suppressionList">
        <item>displayRowNumbers</item>
        <item>classField</item>
        <item>count</item>
        <item>afterLabel</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="SingleValue">
            <param name="classField">range</param>
            <param name="afterLabel"> flights</param>
            <module name="Gimp"/>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="HiddenSavedSearch" layoutPanel="panel_row3_col1" group="Flights by Departure Time from NY" autoRun="True">
    <param name="savedSearch">Flights by Departure Time from NY</param>
    <param name="groupLabel">Flights by Departure Time from NY</param>
    <module name="ViewstateAdapter">
      <param name="savedSearch">Flights by Departure Time from NY</param>
      <param name="suppressionList">
        <item>charting.chart</item>
        <item>displayRowNumbers</item>
        <item>count</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="HiddenChartFormatter">
              <param name="charting.chart">column</param>
              <module name="JSChart">
                <param name="width">100%</param>
                <module name="Gimp"/>
                <module name="ConvertToDrilldownSearch">
                  <module name="ViewRedirector">
                    <param name="viewTarget">flashtimeline</param>
                  </module>
                </module>
              </module>
              <module name="ViewRedirectorLink">
                <param name="viewTarget">flashtimeline</param>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>

<module name="HiddenSavedSearch" layoutPanel="panel_row3_col2" group="Flights by Arrival Time to NY" autoRun="True">
    <param name="savedSearch">Flights by Arrival Time to NY</param>
    <param name="groupLabel">Flights by Arrival Time to NY</param>
    <module name="ViewstateAdapter">
      <param name="savedSearch">Flights by Arrival Time to NY</param>
      <param name="suppressionList">
        <item>charting.chart</item>
        <item>displayRowNumbers</item>
        <item>count</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="HiddenChartFormatter">
              <param name="charting.chart">column</param>
              <module name="JSChart">
                <param name="width">100%</param>
                <module name="Gimp"/>
                <module name="ConvertToDrilldownSearch">
                  <module name="ViewRedirector">
                    <param name="viewTarget">flashtimeline</param>
                  </module>
                </module>
              </module>
              <module name="ViewRedirectorLink">
                <param name="viewTarget">flashtimeline</param>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>

  <module name="HiddenSavedSearch" layoutPanel="panel_row4_col1" group="Flights by Weekday from NY" autoRun="True">
    <param name="savedSearch">Flights per Weekday (NY)</param>
    <param name="groupLabel">Flights by Weekday from NY</param>
    <module name="ViewstateAdapter">
      <param name="savedSearch">Flights per Weekday (NY)</param>
      <param name="suppressionList">
        <item>charting.chart</item>
        <item>displayRowNumbers</item>
        <item>charting.chart.nullValueMode</item>
        <item>charting.layout.splitSeries</item>
        <item>charting.axisY.minimumNumber</item>
        <item>charting.secondaryAxis.maximumNumber</item>
        <item>charting.chart.stackMode</item>
        <item>charting.secondaryAxis.minimumNumber</item>
        <item>charting.legend.placement</item>
        <item>charting.secondaryAxis.scale</item>
        <item>count</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="HiddenChartFormatter">
              <param name="charting.chart">line</param>
              <param name="charting.chart.nullValueMode">gaps</param>
              <param name="charting.layout.splitSeries">false</param>
              <param name="charting.axisY.minimumNumber">1500</param>
              <param name="charting.secondaryAxis.maximumNumber">""</param>
              <param name="charting.chart.stackMode">default</param>
              <param name="charting.legend.placement">right</param>
              <param name="charting.secondaryAxis.scale">""</param>
              <module name="JSChart">
                <param name="width">100%</param>
                <module name="Gimp"/>
                <module name="ConvertToDrilldownSearch">
                  <module name="ViewRedirector">
                    <param name="viewTarget">flashtimeline</param>
                  </module>
                </module>
              </module>
              <module name="ViewRedirectorLink">
                <param name="viewTarget">flashtimeline</param>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>

  <module name="HiddenSavedSearch" layoutPanel="panel_row5_col1" group="Airline Traffic Distribution in NY by weekday" autoRun="True">
    <param name="savedSearch">Airline Traffic Distribution in NY by weekday</param>
    <param name="groupLabel">Airline Traffic Distribution in NY by weekday</param>
    <module name="ViewstateAdapter">
      <param name="savedSearch">Airline Traffic Distribution in NY by weekday</param>
      <param name="suppressionList">
        <item>charting.chart</item>
        <item>displayRowNumbers</item>
        <item>count</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="HiddenChartFormatter">
              <param name="charting.chart">column</param>
              <module name="JSChart">
                <param name="width">100%</param>
                <module name="Gimp"/>
                <module name="ConvertToDrilldownSearch">
                  <module name="ViewRedirector">
                    <param name="viewTarget">flashtimeline</param>
                  </module>
                </module>
              </module>
              <module name="ViewRedirectorLink">
                <param name="viewTarget">flashtimeline</param>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>

  </module>
</view>

