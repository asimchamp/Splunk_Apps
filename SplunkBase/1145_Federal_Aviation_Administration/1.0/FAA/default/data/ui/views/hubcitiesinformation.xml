<view autoCancelInterval="90" isVisible="true" objectMode="SimpleDashboard" onunloadCancelJobs="true" refresh="-1" template="dashboard.html">
  <label>Hub Cities Information</label>
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="DashboardTitleBar" layoutPanel="viewHeader"/>
  <module name="Message" layoutPanel="navigationHeader">
    <param name="level">warn</param>
    <param name="filter">splunk.search.job</param>
    <param name="clearOnJobDispatch">True</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row1_col1">
    <param name="searchWhenChanged">True</param>
    <param name="default">All Time</param>
  <module name="HiddenSavedSearch" layoutPanel="panel_row2_col1" group="Top Hub Cities" autoRun="True">
    <param name="savedSearch">Top Hub Cities</param>
    <param name="groupLabel">Top Hub Cities</param>
    <module name="ViewstateAdapter">
      <param name="savedSearch">Top Hub Cities</param>
      <param name="suppressionList"/>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator"/>
        <module name="Paginator">
          <param name="count">10</param>
          <param name="entityName">results</param>
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="SimpleResultsTable">
              <param name="allowTransformedFieldSelect">True</param>
              <param name="drilldown">row</param>
              <param name="entityName">results</param>
              <module name="Gimp"/>
              <module name="ConvertToDrilldownSearch">
                <module name="ViewRedirector">
                  <param name="viewTarget">flashtimeline</param>
                </module>
              </module>
            </module>
            <module name="ViewRedirectorLink">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="HiddenSearch" layoutPanel="panel_row3_col1" group="Airline Distribution over Hub Ctiies" autoRun="True">
    <param name="search">[search sourcetype="airline_csv" | stats dc(AirlineID) AS dcAirline, count by OriginCityName | eval hubscore=(dcAirline * log(count)) | sort - hubscore | head 10 | table OriginCityName] | chart count over OriginCityName by AirlineDescription usenull=f</param>
    <param name="groupLabel">Airline Distribution over Hub Ctiies</param>
    <module name="ViewstateAdapter">
      <param name="suppressionList">
        <item>charting.chart</item>
        <item>displayRowNumbers</item>
        <item>count</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="HiddenChartFormatter">
              <param name="charting.chart">column</param>
              <module name="JSChart">
                <param name="width">100%</param>
                <module name="Gimp"/>
                <module name="ConvertToDrilldownSearch">
                  <module name="ViewRedirector">
                    <param name="viewTarget">flashtimeline</param>
                  </module>
                </module>
              </module>
              <module name="ViewRedirectorLink">
                <param name="viewTarget">flashtimeline</param>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="HiddenSearch" layoutPanel="panel_row4_col1" group="Hub City Distribution over Airlines" autoRun="True">
    <param name="search">[search sourcetype="airline_csv" | stats dc(AirlineID) AS dcAirline, count by OriginCityName | eval hubscore=(dcAirline * log(count)) | sort - hubscore | head 10 | table OriginCityName] | chart count over AirlineDescription by OriginCityName usenull=f</param>
    <param name="groupLabel">Hub City Distribution over Airlines</param>
    <module name="ViewstateAdapter">
      <param name="suppressionList">
        <item>charting.chart</item>
        <item>displayRowNumbers</item>
        <item>count</item>
      </param>
      <module name="HiddenFieldPicker">
        <param name="strictMode">True</param>
        <module name="JobProgressIndicator">
          <module name="EnablePreview">
            <param name="display">False</param>
            <param name="enable">True</param>
            <module name="HiddenChartFormatter">
              <param name="charting.chart">column</param>
              <module name="JSChart">
                <param name="width">100%</param>
                <module name="Gimp"/>
                <module name="ConvertToDrilldownSearch">
                  <module name="ViewRedirector">
                    <param name="viewTarget">flashtimeline</param>
                  </module>
                </module>
              </module>
              <module name="ViewRedirectorLink">
                <param name="viewTarget">flashtimeline</param>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  </module>
</view>

