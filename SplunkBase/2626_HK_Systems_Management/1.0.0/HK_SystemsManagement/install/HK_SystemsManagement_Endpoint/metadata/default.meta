# Default values that can be overridden for specific stanzas
[]
access = read : [ * ], write : [ admin ]
export = none

### VIEWSTATES: even normal users should be able to create shared viewstates
[viewstates]
access = read : [ * ], write : [ * ]
export = system

# Custom search commands should be usable in the search app, too
[commands]
export = system