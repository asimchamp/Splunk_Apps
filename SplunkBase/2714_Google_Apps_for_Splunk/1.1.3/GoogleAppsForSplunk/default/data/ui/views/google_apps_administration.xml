<form>
  <label>Google Apps Administration</label>
  <searchTemplate></searchTemplate>
  <fieldset>
    <input type="time" token="domain_time">
      <label>Time Range</label>
      <default>
        <earliest>-7d@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Google Apps Domains</title>
        <search>
          <query>|`gapps_save_domains`</query>
          <earliest>$domain_time.earliest$</earliest>
          <latest>$domain_time.latest$</latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">undefined</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Allowed Administrators</title>
        <search>
          <query>index=_internal | head 10 | top sourcetype</query>
          <earliest>$domain_time.earliest$</earliest>
          <latest>$domain_time.latest$</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.axisY2.enabled">undefined</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Allowed IP Addresses</title>
        <search>
          <query>index=_internal | head 10 | top source</query>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.axisY2.enabled">undefined</option>
      </chart>
    </panel>
  </row>
</form>