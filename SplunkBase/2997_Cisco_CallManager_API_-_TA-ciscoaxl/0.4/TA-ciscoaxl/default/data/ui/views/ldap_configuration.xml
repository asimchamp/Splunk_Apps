<dashboard>
  <label>LDAP configuration</label>
  <row>
    <panel>
      <title>List LDAP directories</title>
      <table>
        <search>
          <query>| ciscoaxl listLdapDirectory name="%" columns="name,ldapDn,userSearchBase" | table name,ldapDn,userSearchBase</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>List LDAP filters</title>
      <table>
        <search>
          <query>| ciscoaxl listLdapFilter name="%" columns="name,filter" | table name,filter</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
      </table>
    </panel>
  </row>
</dashboard>