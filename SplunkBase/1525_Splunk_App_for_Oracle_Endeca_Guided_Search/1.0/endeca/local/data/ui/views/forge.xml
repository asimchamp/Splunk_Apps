<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Forge</label>
  <row>
    <chart>
      <searchName>Latest Forge Completion Percentage</searchName>
      <title>Latest Forge Completion Percentage</title>
      <option name="charting.chart">fillerGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.rangeValues">[0,100]</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.gaugeColors">[0xf7912c]</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <chart>
      <searchName>Averge Forge Duration</searchName>
      <title>Averge Forge Duration</title>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">zero</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.primaryAxisTitle.text"/>
    </chart>
  </row>
  <row>
    <event>
      <searchName>Raw Forge Logs</searchName>
      <title>Raw Forge Logs</title>
    </event>
  </row>
</dashboard>
