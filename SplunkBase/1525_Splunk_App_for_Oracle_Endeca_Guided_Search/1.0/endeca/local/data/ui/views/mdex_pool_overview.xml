<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>MDEX Pool Overview</label>
  <row>
    <chart>
      <searchName>Average Navigation and Search Throughput Last 15 Minutes</searchName>
      <title>Avg Navigation and Search Throughput Last 15 Min (ops/sec)</title>
      <option name="charting.chart">radialGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <chart>
      <searchName>Average Dimension Search Throughput Last 15 Minutes</searchName>
      <title>Avg Dimension Search Throughput Last 15 Min (ops/sec)</title>
      <option name="charting.chart">radialGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <chart>
      <searchName>Average Record Detail Throughput Last 15 Minutes</searchName>
      <title>Avg Record Detail Throughput Last 15 Min (ops/sec)</title>
      <option name="charting.chart">radialGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Average Total Request Time by MDEX</searchName>
      <title>Average Total Request Time by MDEX</title>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Request Time (ms)</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">bottom</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
    <chart>
      <searchName>Average Total Processing Time by MDEX</searchName>
      <title>Average Total Processing Time by MDEX</title>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Processing Time (ms)</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">bottom</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Total Queries by Type</searchName>
      <title>Total Queries by Type</title>
      <option name="charting.chart">pie</option>
    </chart>
    <chart>
      <searchName>Total Query Processing Time by Type</searchName>
      <title>Total Query Processing Time by Type</title>
      <option name="charting.chart">pie</option>
    </chart>
  </row>
</dashboard>
