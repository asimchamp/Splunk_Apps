<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Site Overview</label>
  <row>
    <single>
      <searchName>Total Visits Today</searchName>
      <title>Total Visits Today</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
    <single>
      <searchName>Average Pages Per Visit Today</searchName>
      <title>Average Pages Per Visit Today</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
    <single>
      <searchName>Average Searches Per Visit Today</searchName>
      <title>Average Searches Per Visit Today</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
  </row>
  <row>
    <table>
      <searchName>Top Search Terms Past 30 Days</searchName>
      <title>Top Search Terms Past 30 Days</title>
    </table>
    <chart>
      <searchName>Search Volume Past 30 Days</searchName>
      <title>Search Volume Past 30 Days</title>
      <option name="charting.chart">column</option>
    </chart>
  </row>
  <row>
    <table>
      <searchName>Visit Type Overview Past 30 Days</searchName>
      <title>Visit Type Overview Past 30 Days</title>
    </table>
    <chart>
      <searchName>Visit Type Contribution Past 30 Days</searchName>
      <title>Visit Type Contribution Past 30 Days</title>
      <option name="charting.chart">pie</option>
    </chart>
  </row>
</dashboard>
