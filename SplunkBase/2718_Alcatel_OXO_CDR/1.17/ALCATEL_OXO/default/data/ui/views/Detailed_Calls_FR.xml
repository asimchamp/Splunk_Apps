<form>
  <label>Appels Détaillés Format Brut</label>
  <fieldset submitButton="false"></fieldset>
  <row>
    <panel>
      <input type="time" token="DetailCall" searchWhenChanged="true">
        <label>Période</label>
        <default>
          <earliest>0</earliest>
          <latest></latest>
        </default>
      </input>
      <input type="text" token="Token_OXO_Call_Duration" searchWhenChanged="true">
        <label>Durée</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="Token_OXO_User" searchWhenChanged="true">
        <label>Utilisateur</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="Token_OXO_User_Charged" searchWhenChanged="true">
        <label>Utilisateur Interne</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="Token_OXO_User_Name" searchWhenChanged="true">
        <label>Nom Utilisateur</label>
        <default>*</default>
      </input>
      <input type="dropdown" token="Token_OXO_Direction" searchWhenChanged="true">
        <label>Sens de l'Appel</label>
        <default>*</default>
        <choice value="*">Tout</choice>
        <choice value="&quot;--&gt;&quot;">Sortant</choice>
        <choice value="&quot;&lt;--&quot;">Entrant</choice>
      </input>
      <input type="dropdown" token="Token_OXO_Line" searchWhenChanged="true">
        <label>Type de Ligne</label>
        <default>*</default>
        <choice value="*">Tout</choice>
        <choice value="L*">Analogique</choice>
        <choice value="N*">Numérique TO</choice>
        <choice value="P*">Numérique T2</choice>
        <choice value="V*">VoIP</choice>
        <choice value="CH">Réveil Chambre</choice>
      </input>
      <input type="text" token="Token_OXO_Unit" searchWhenChanged="true">
        <label>Unité</label>
        <default>*</default>
      </input>
      <input type="dropdown" token="Token_OXO_Service" searchWhenChanged="true">
        <label>Service</label>
        <default>*</default>
        <initialValue>*</initialValue>
        <choice value="*">Tout</choice>
        <choice value="ST">Téléphonie</choice>
        <choice value="T*">Donnée</choice>
      </input>
      <input type="dropdown" token="Token_OXO_Complement" searchWhenChanged="true">
        <label>Complément de Service</label>
        <default>*</default>
        <initialValue>*</initialValue>
        <choice value="*">Tout</choice>
        <choice value="*X*">Transfert</choice>
        <choice value="*R*">Renvoi Externe</choice>
        <choice value="*N*">Renvoi PCX</choice>
        <choice value="*S*">DISA-Accès Direct en Entrée Système</choice>
        <choice value="*T*">Télétaxe Activée</choice>
        <choice value="*I*">Signalisation Utilisateur</choice>
      </input>
      <input type="text" token="Token_OXO_Dialled_Number" searchWhenChanged="true">
        <label>Numéro Composé</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="dropdown" token="Token_OXO_Mode" searchWhenChanged="true">
        <label>Mode de Numérotation</label>
        <default>*</default>
        <initialValue>*</initialValue>
        <choice value="*">Tout</choice>
        <choice value="M">Numérotation Manuelle</choice>
        <choice value="I">Code Court Personnel</choice>
        <choice value="R">Code Court Commun</choice>
      </input>
      <input type="text" token="Token_OXO_Cost" searchWhenChanged="true">
        <label>Coût</label>
        <default>*</default>
      </input>
      <input type="text" token="Token_Business_Code">
        <label>Code Affaire</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <table>
        <title>Appels Détaillés</title>
        <search>
          <query>`sourcelog`   
(OXO_Direction="--&gt;" OR OXO_Direction="&lt;--")
OXO_Unit=$Token_OXO_Unit$  OXO_Dialled_Number=$Token_OXO_Dialled_Number$  OXO_Complement=$Token_OXO_Complement$ OXO_Dialling_Mode=$Token_OXO_Mode$ OXO_Service=$Token_OXO_Service$ OXO_Cost=$Token_OXO_Cost$ OXO_User=$Token_OXO_User$  OXO_User_Charged=$Token_OXO_User_Charged$ OXO_Call_Duration=$Token_OXO_Call_Duration$ OXO_Direction=$Token_OXO_Direction$  OXO_Line=$Token_OXO_Line$ OXO_Call_Duration=$Token_OXO_Call_Duration$ OXO_Business_Code=$Token_Business_Code$ OXO_User_Name=$Token_OXO_User_Name$
| `FR_DetailCall_RENAME`  
| `FR_DetailCall_Table`</query>
          <earliest>$DetailCall.earliest$</earliest>
          <latest>$DetailCall.latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
</form>
