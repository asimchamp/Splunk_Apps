<form>
  <label>DISA - Taking Control of Remote PBX</label>
  <fieldset submitButton="false"></fieldset>
  <row>
    <panel>
      <input type="time" token="DetailCall" searchWhenChanged="true">
        <label>Epoch</label>
        <default>
          <earliest>0</earliest>
          <latest></latest>
        </default>
      </input>
      <input type="text" token="Token_OXO_Call_Duration" searchWhenChanged="true">
        <label>Call Duration</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="Token_OXO_User" searchWhenChanged="true">
        <label>User</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="Token_OXO_User_Charged" searchWhenChanged="true">
        <label>Internal User</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="Token_OXO_User_Name" searchWhenChanged="true">
        <label>User Name</label>
        <default>*</default>
      </input>
      <input type="dropdown" token="Token_OXO_Direction" searchWhenChanged="true">
        <label>Call Direction</label>
        <default>*</default>
        <choice value="*">All</choice>
        <choice value="&quot;--&gt;&quot;">Outgoing</choice>
        <choice value="&quot;&lt;--&quot;">Incoming</choice>
      </input>
      <input type="dropdown" token="Token_OXO_Line" searchWhenChanged="true">
        <label>Line Type</label>
        <default>*</default>
        <choice value="*">All</choice>
        <choice value="L*">Analog</choice>
        <choice value="N*">Digital BRI</choice>
        <choice value="P*">Digital E1</choice>
        <choice value="V*">VoIP</choice>
        <choice value="CH">Alarm Clock</choice>
      </input>
      <input type="text" token="Token_OXO_Unit" searchWhenChanged="true">
        <label>Unit</label>
        <default>*</default>
      </input>
      <input type="dropdown" token="Token_OXO_Service" searchWhenChanged="true">
        <label>Service</label>
        <default>*</default>
        <initialValue>*</initialValue>
        <choice value="*">All</choice>
        <choice value="ST">Telephony</choice>
        <choice value="T*">Data</choice>
      </input>
      <input type="dropdown" searchWhenChanged="true" token="Token_OXO_Complement">
        <label>Complement</label>
        <default>*S*</default>
        <initialValue>*</initialValue>
        <choice value="*">All</choice>
        <choice value="*X*">Forward</choice>
        <choice value="*R*">Forward to External</choice>
        <choice value="*N*">PCX Forward</choice>
        <choice value="*S*">DISA - Direct Inward System Access</choice>
        <choice value="*T*">Counting activated</choice>
        <choice value="*I*">User Signalisation</choice>
      </input>
      <input type="text" token="Token_OXO_Dialled_Number" searchWhenChanged="true">
        <label>Dialled Number</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="dropdown" searchWhenChanged="true" token="Token_OXO_Mode">
        <label>Dialling Mode</label>
        <default>*</default>
        <initialValue>*</initialValue>
        <choice value="*">All</choice>
        <choice value="M">Manual dialing</choice>
        <choice value="I">Private Short Code</choice>
        <choice value="R">Public Short Code</choice>
      </input>
      <input type="text" token="Token_OXO_Cost" searchWhenChanged="true">
        <label>Cost</label>
        <default>*</default>
      </input>
      <input type="text" token="Token_Business_Code" searchWhenChanged="true">
        <label>Business Code</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <table>
        <title>Detailed Calls</title>
        <search>
          <query>`sourcelog`  
(OXO_Direction="--&gt;" OR OXO_Direction="&lt;--")
OXO_Unit=$Token_OXO_Unit$
 OXO_Dialled_Number=$Token_OXO_Dialled_Number$  OXO_Complement=$Token_OXO_Complement$ OXO_Dialling_Mode=$Token_OXO_Mode$ OXO_Service=$Token_OXO_Service$ OXO_Cost=$Token_OXO_Cost$ OXO_User=$Token_OXO_User$  OXO_User_Charged=$Token_OXO_User_Charged$ OXO_Call_Duration=$Token_OXO_Call_Duration$ OXO_Direction=$Token_OXO_Direction$	 OXO_Line=$Token_OXO_Line$ OXO_Call_Duration=$Token_OXO_Call_Duration$ OXO_Business_Code=$Token_Business_Code$ OXO_User_Name=$Token_OXO_User_Name$
| `US_DetailCall_RENAME`
| `US_DetailCall_Table`</query>
          <earliest>$DetailCall.earliest$</earliest>
          <latest>$DetailCall.latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
        <format type="color" field="User Name">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
      </table>
    </panel>
  </row>
</form>
