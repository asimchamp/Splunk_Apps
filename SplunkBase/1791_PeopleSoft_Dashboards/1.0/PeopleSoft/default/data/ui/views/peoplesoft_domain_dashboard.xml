<?xml version="1.0" encoding="utf-8"?><form class="formsearch">
  <label>PeopleSoft Domain Dashboard</label>
  <description/>
  <fieldset>
    <input type="dropdown" token="instance" searchWhenChanged="true">
      <label>Domain selection:</label>
      <!--<default>CS90PRD</default>-->
      <populatingSearch fieldForValue="instance" fieldForLabel="instance">
        <![CDATA[`ps_indexes` `ps_servers` sourcetype=status earliest=-1h@h `extract_instance` | search `instance_filter` | dedup instance | eval domain=upper(instance) |dedup domain | rename domain as instance | fields instance | sort instance]]>
      </populatingSearch>
    </input>
    <input type="time" searchWhenChanged="true">
      <default>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
  </fieldset>
  <row>
    <event>
      <title>Latest Status</title>
      <searchString>`ps_indexes` $instance$ sourcetype=status earliest=-15m@m latest=now `extract_instance` | dedup instance, host</searchString>
      <!--      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>-->
      <option name="wrap">true</option>
      <option name="rowNumbers">0</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">row</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">1</option>
      <option name="raw.drilldown">full</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="count">5</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <fields>["host","source","sourcetype"]</fields>
    </event>
    <event>
      <title>Errors</title>
      <searchString>`ps_indexes` ( $instance$ AND `ps_error_search` ) earliest=$earliest$ |eval latest=coalesce("$latest$", now()) `ps_error_filter`</searchString>
      <!--      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>-->
      <option name="list.drilldown">full</option>
      <option name="list.wrap">0</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">0</option>
      <option name="type">list</option>
      <option name="count">5</option>
      <fields>["host","source","sourcetype"]</fields>
    </event>
    <chart>
      <title>Top Pages</title>
      <searchString>`ps_web_index` $instance$ .GBL  earliest=$earliest$ |eval latest=coalesce("$latest$", now()) `extract_page_menu`|top page_name</searchString>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <!--      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>-->
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <event>
      <title>Application Server Logs</title>
      <searchString>`ps_app_index` source=*$instance$* sourcetype=psft_appsrv_appsrv earliest=$earliest$ |eval latest=coalesce("$latest$", now())</searchString>
      <!--      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>-->
      <option name="wrap">true</option>
      <option name="rowNumbers">0</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">cell</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">0</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">0</option>
      <option name="type">list</option>
      <option name="count">10</option>
      <fields>["host","source","sourcetype"]</fields>
    </event>
    <event>
      <title>Process Scheduler Logs</title>
      <searchString>`ps_psched_index` source=*$instance$* NOT sourcetype=status earliest=$earliest$ |eval latest=coalesce("$latest$", now())</searchString>
      <!--      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>-->
      <option name="wrap">true</option>
      <option name="rowNumbers">0</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">cell</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">0</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">0</option>
      <option name="type">list</option>
      <option name="count">10</option>
      <fields>["host","source","sourcetype"]</fields>
    </event>
  </row>
  <row>
    <event>
      <title>Database Server Logs</title>
      <searchString>`ps_db_index` NOT sourcetype=status *$instance$*  earliest=$earliest$ |eval latest=coalesce("$latest$", now())</searchString>
      <earliestTime>0</earliestTime>
      <latestTime>now</latestTime>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">0</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">0</option>
      <option name="type">list</option>
      <option name="count">10</option>
      <fields>["host","source","sourcetype"]</fields>
    </event>
    <event>
      <title>Weblogic Logs</title>
      <searchString>`ps_web_index` $instance$ NOT sourcetype=status earliest=$earliest$ |eval latest=coalesce("$latest$", now())</searchString>
      <!--      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>-->
      <option name="displayRowNumbers">true</option>
      <option name="wrap">true</option>
      <option name="rowNumbers">0</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">row</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">0</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="count">10</option>
      <fields>["host","source","sourcetype"]</fields>
    </event>
  </row>
</form>