<form>
<label>Search for success and failures over time</label>


<searchTemplate>index=soae_transacion WorkflowName!=NULL | soaekv | timechart count by "/logEntry/body/subEntry/body/wflSummary/completionStatus/status"</searchTemplate>
<fieldset autoRun="true" >
  <input type="time">
  <label>Select Time Range</label>
    <default>Last 7 days</default>
  </input>
  
</fieldset>

<row>
  <chart>
    <title>Success and Failures over the </title>
    <option name="charting.chart">column</option>
    <option name="charting.primaryAxisTitle">ktm</option>
    <option name="charting.secondaryAxisTitle">time</option>
    <option name="charting.legend.labels">[success,fail]</option>
    <option name="charting.chart.stackMode">stacked</option>
    <option name="charting.seriesColors">[0x00FF00,0xFF0000,0x0000FF]</option>
  </chart>
</row>

</form>
