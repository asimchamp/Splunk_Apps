  <module name="HiddenSavedSearch" layoutPanel="panel_row2_col3" group="Activity Level by Network Share" autoRun="True">
    <param name="savedSearch">Activity Level by Network Volume</param>
    <param name="groupLabel">Activity Level by Network Share</param>
    <module name="TimeRangePicker">
      <module name="ViewstateAdapter">
        <param name="suppressionList">
          <item>charting.chart.stackMode</item>
          <item>count</item>
          <item>charting.chart</item>
          <item>displayRowNumbers</item>
        </param>
        <param name="savedSearch">Activity Level by Network Volume</param>
        <module name="HiddenFieldPicker">
          <param name="strictMode">True</param>
          <module name="JobProgressIndicator">
            <module name="EnablePreview">
              <param name="enable">True</param>
              <param name="display">False</param>
              <module name="HiddenChartFormatter">
                <param name="charting.chart.stackMode">stacked100</param>
                <param name="charting.chart">bar</param>
                <module name="FlashChart">
                  <param name="width">100%</param>
                  <!-- CUSTOMIZED DRILLDOWN -->
                  <module name="HiddenSearch">
                    <module name="HiddenSearch">
                      <param name="search">
                      index="ftauditevent*" ftstartfileidvolumeidtype="*Network*"
                      OR ftendfileidvolumeidtype="*Network*"
                      OR ftfileidvolumeidtype="*Network*"
                      OR ftfileidvolumetype="*Network*"
                      OR ftstartfileidvolumetype="*Network*"
                      OR ftendfileidvolumetype="*Network*"
                      | eval Id=coalesce(ftstartfileid, ftfileid)
                      | eval EndFileId=coalesce(ftendfileid, ftfileid)
                      | eval File=coalesce(ftstartfileidpath,ftsourcefileidpath,ftfileidpath)
                      | eval EndFile=coalesce(ftendfileidpath,ftfileidpath)
                      | eval Volume=coalesce(ftstartfileidvolumeidname, ftstartfileidvolume, ftsourcevolumeidname, ftfileidvolumeidname, ftfileidvolume)
                      | eval EndVolume=coalesce(ftendfileidvolumeidname, ftendfileidvolume, ftfileidvolumeidname, ftfileidvolume)
                      | eval Action=coalesce(ftlinktype, fteventtype)
                      | rename ftendfileidpath AS EndFile ftendfileid AS EndFileId
                      | rename ftsourceuseridname AS User ftsourceuserid AS UserId
                      | rename ftsourcemachineidname AS Machine ftsourcemachineid AS MachineId
                      | rename fteventtime AS Date
                      | fields Id, File, Volume, Action, EndFileId, EndFile, EndVolume, UserId, User, MachineId, Machine, Date | fields - _*
                      | search Volume="$_REPLACE_VOLUME_$" EndVolume="$_REPLACE_END_VOLUME_$"
                      </param>
                      <module name="ConvertToIntention">
                        <param name="intention">
                          <param name="name">stringreplace</param>
                          <param name="arg"> <param name="_REPLACE_VOLUME_"> <param name="value">$click.value$</param> </param> </param>
                        </param>
                        <module name="ConvertToIntention">
                          <param name="intention">
                            <param name="name">stringreplace</param>
                            <param name="arg"> <param name="_REPLACE_END_VOLUME_"> <param name="value">$click.name2$</param> </param> </param>
                          </param>
                          <module name="JobProgressIndicator"></module>
                          <module name="FTResultTable"></module>
                        </module> <!-- Conv to Intention 2 -->
                      </module> <!-- Conv to Intention 1 -->
                    </module> <!-- hidden search 2 -->
                  </module> <!-- Hidden search -->
                </module>
                <module name="ViewRedirectorLink">
                  <param name="viewTarget">flashtimeline</param>
                </module>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="NullModule" layoutPanel="panel_row3_col1" group="Chart Details" autoRun="False"></module>
</view>

