<view autoCancelInterval="90" isVisible="true" objectMode="SimpleDashboard" onunloadCancelJobs="true" refresh="-1" template="dashboard.html">
  <label>Dashboard</label>
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="Message" layoutPanel="navigationHeader">
    <param name="filter">splunk.search.job</param>
    <param name="clearOnJobDispatch">True</param>
    <param name="maxSize">1</param>
  </module>
  <module name="HiddenSavedSearch" layoutPanel="panel_row1_col1" group="Exit Point Activity Over Time" autoRun="True">
    <param name="savedSearch">Exit Point Activity Over Time</param>
    <param name="groupLabel">Exit Point Activity Over Time</param>
    <module name="TimeRangePicker">
      <module name="ViewstateAdapter">
        <param name="suppressionList">
          <item>charting.chart.stackMode</item>
          <item>charting.chart</item>
          <item>charting.secondaryAxisTitle.text</item>
          <item>charting.legend.placement</item>
          <item>charting.primaryAxisTitle.text</item>
          <item>charting.chart.nullValueMode</item>
          <item>charting.secondaryAxis.scale</item>
          <item>charting.layout.splitSeries</item>
        </param>
        <param name="savedSearch">Exit Point Activity Over Time</param>
        <module name="HiddenFieldPicker">
          <param name="strictMode">True</param>
          <module name="JobProgressIndicator">
            <module name="EnablePreview">
              <param name="enable">True</param>
              <param name="display">False</param>
              <module name="HiddenChartFormatter">
                <param name="charting.chart.stackMode">stacked</param>
                <param name="charting.chart">column</param>
                <param name="charting.secondaryAxisTitle.text">Number of Events</param>
                <param name="charting.legend.placement">bottom</param>
                <param name="charting.primaryAxisTitle.text">Date</param>
                <param name="charting.chart.nullValueMode">zero</param>
                <param name="charting.secondaryAxis.scale">log</param>
                <param name="charting.layout.splitSeries">false</param>
                <module name="FlashChart">
                  <param name="width">100%</param>
                  <!-- CUSTOMIZED DRILLDOWN -->
                  <module name="HiddenSearch">
                    <module name="ConvertToIntention">
                      <param name="intention">
                        <param name="name">addterm</param>
                        <param name="arg">
                          <param name="eventtype">$click.name2$</param>
                        </param>
                        <param name="flags"><list>indexed</list></param>
                      </param>
                      <module name="HiddenSearch">
                        <param name="search">
                          index="ftauditevent*" 
                          | eval Id=coalesce(ftstartfileid,ftfileid)
                          | eval File=coalesce(ftstartfileidpath,ftfileidpath)
                          | eval Destination=coalesce(ftendfileidpath,ftrecipients,ftwebsitedetailslist,ftprintername)
                          | eval Action=coalesce(ftlinktype, fteventtype)
                          | rename ftendfileid AS EndFileId
                          | rename ftsourceuseridname as User ftsourceuserid as UserId
                          | rename ftsourcemachineidname as Machine ftsourcemachineid as MachineId
                          | fields Id, File, Action, EndFileId, Destination, UserId, User, MachineId, Machine, fteventtime | fields - _*
                        </param>
                        <module name="JobProgressIndicator"></module>
                        <module name="FTResultTable"></module>
                      </module> <!-- hidden search 2 -->
                    </module> <!-- convert to intention -->
                  </module> <!-- Hidden search -->
                </module>
                <module name="ViewRedirectorLink">
                  <param name="viewTarget">flashtimeline</param>
                </module>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="HiddenSavedSearch" layoutPanel="panel_row1_col2" group="Hour of Day Exit Point Activity" autoRun="True">
    <param name="savedSearch">Daily Exit Point Activity</param>
