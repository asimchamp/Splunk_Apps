<?xml version='1.0' encoding='utf-8'?>
<form version="1.1" script="main.js, process_detail.js" stylesheet="main.css, process_detail.css">

   <label>Process Performance</label>
   <description>This dashboard displays detailed information about process performance.</description>

   <fieldset autoRun="true">
      <input type="time" searchWhenChanged="true">
         <label>Time range:</label>
         <default>
            <earliest>-1h</earliest>
            <latest>now</latest>
         </default>
         <change>
            <eval token="TimeRangePickerSpan">relative_time(now(), latest) - relative_time(now(), earliest)</eval>
            <eval token="PivotPeriodAutoSolo">case('TimeRangePickerSpan' &lt; 350000, "auto", 'TimeRangePickerSpan' &lt; 610000, "hour", 1=1, "auto")</eval>
            <eval token="PivotPeriodAutoTimechart">case('TimeRangePickerSpan' &lt; 1200, "second", 'TimeRangePickerSpan' &lt; 300000, "minute", 'TimeRangePickerSpan' &lt; 700000, "hour", 1=1, "day")</eval>
         </change>
      </input>
      <input type="dropdown" token="FilterField" id="Input_FilterField">
         <label>Filter field:</label>
      </input>
      <input type="dropdown" token="FilterOperator" id="Input_FilterOperator">

         <label>Filter operator:</label>
      </input>
      <input type="text" token="FilterExpression" id="Input_FilterExpression">
         <label>Filter expression:</label>
         <default>*</default>
      </input>
      <input type="dropdown" token="FilterField2" id="Input_FilterField2" depends="$FilterLevel2$">
      </input>
      <input type="dropdown" token="FilterOperator2" id="Input_FilterOperator2" depends="$FilterLevel2$">
      </input>
      <input type="text" token="FilterExpression2" id="Input_FilterExpression2" depends="$FilterLevel2$">
      </input>
      <input type="dropdown" token="FilterField3" id="Input_FilterField3" depends="$FilterLevel3$">
      </input>
      <input type="dropdown" token="FilterOperator3" id="Input_FilterOperator3" depends="$FilterLevel3$">
      </input>
      <input type="text" token="FilterExpression3" id="Input_FilterExpression3" depends="$FilterLevel3$">
      </input>
      <input type="dropdown" token="FilterField4" id="Input_FilterField4" depends="$FilterLevel4$">
      </input>
      <input type="dropdown" token="FilterOperator4" id="Input_FilterOperator4" depends="$FilterLevel4$">
      </input>
      <input type="text" token="FilterExpression4" id="Input_FilterExpression4" depends="$FilterLevel4$">
      </input>
      <input type="dropdown" token="FilterField5" id="Input_FilterField5" depends="$FilterLevel5$">
      </input>
      <input type="dropdown" token="FilterOperator5" id="Input_FilterOperator5" depends="$FilterLevel5$">
      </input>
      <input type="text" token="FilterExpression5" id="Input_FilterExpression5" depends="$FilterLevel5$">
      </input>
      <input type="dropdown" token="FilterField6" id="Input_FilterField6" depends="$FilterLevel6$">
      </input>
      <input type="dropdown" token="FilterOperator6" id="Input_FilterOperator6" depends="$FilterLevel6$">
      </input>
      <input type="text" token="FilterExpression6" id="Input_FilterExpression6" depends="$FilterLevel6$">
      </input>
      <input type="text" token="FilterLevels" id="Input_FilterLevels" depends="$FilterLevels_Hidden$">
      </input>
      <input type="text" token="FilterDatamodel" id="Input_FilterDatamodel" depends="$FilterDatamodel_Hidden$">
         <default>Process_ProcessDetail</default>
      </input>
   </fieldset>

   <search id="Search_Charts">
      <query>
      </query>
   </search>
   
   <!-- Row 1 -->
   <row>

      <!-- Column 1 -->
      <panel>
         <title>CPU per $ModeLower$ (top 10)</title>
         <input type="dropdown" token="Panel11Function1" id="Input_Panel11Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <input type="dropdown" token="Panel11Field1" id="Input_Panel11Field1" searchWhenChanged="true">
            <choice value="ProcCPUTimeS">CPU time (s)</choice>
            <choice value="ProcCPUPercent">CPU usage (%)</choice>
            <default>ProcCPUPercent</default>
            <label>Field:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart id="Chart_Panel11">
            <search base="Search_Charts">
               <query>
                  sort limit=10 -"$Panel11Function1$($Panel11Field1$)" 
                  | fields "$ModeGroupByAs$" "$Panel11Function1$($Panel11Field1$)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>

      <!-- Column 2 -->
      <panel>
         <title>IO per $ModeLower$ (top 10)</title>
         <input type="dropdown" token="Panel12Function1" id="Input_Panel12Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>sum</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <input type="dropdown" token="Panel12Field1" id="Input_Panel12Field1" searchWhenChanged="true">
            <choice value="ProcIOCount">IO r+w count</choice>
            <choice value="ProcIOReadCount">IO read count</choice>
            <choice value="ProcIOWriteCount">IO write count</choice>
            <choice value="ProcIOMB">IO r+w volume (MB)</choice>
            <choice value="ProcIOReadMB">IO read volume (MB)</choice>
            <choice value="ProcIOWriteMB">IO write volume (MB)</choice>
            <choice value="ProcIODurationMs">IO r+w duration (ms)</choice>
            <choice value="ProcIODurationReadMs">IO read duration (ms)</choice>
            <choice value="ProcIODurationWriteMs">IO write duration (ms)</choice>
            <choice value="ProcIOPS">IOPS r+w</choice>
            <choice value="ProcIOPSRead">IOPS read</choice>
            <choice value="ProcIOPSWrite">IOPS write</choice>
            <choice value="ProcIOMBPS">thruput r+w (MB/s)</choice>
            <choice value="ProcIOLatencyMs">latency r+w (ms)</choice>
            <choice value="ProcIOLatencyReadMs">latency read (ms)</choice>
            <choice value="ProcIOLatencyWriteMs">latency write (ms)</choice>
            <default>ProcIOCount</default>
            <label>Field:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart id="Chart_Panel12">
            <search base="Search_Charts">
               <query>
                  sort limit=10 -"$Panel12Function1$($Panel12Field1$)" 
                  | fields "$ModeGroupByAs$" "$Panel12Function1$($Panel12Field1$)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>
   </row>

   <!-- Row 2 -->
   <row>

      <!-- Column 1 -->
      <panel>
         <title>RAM per $ModeLower$ (top 10)</title>
         <input type="dropdown" token="Panel21Function1" id="Input_Panel21Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart id="Chart_Panel21">
            <search base="Search_Charts">
               <query>
                  sort limit=10 -"$Panel21Function1$(ProcWorkingSetMB)" 
                  | fields "$ModeGroupByAs$" "$Panel21Function1$(ProcWorkingSetMB)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>

      <!-- Column 2 -->
      <panel>
         <title>Other metrics per $ModeLower$ (top 10)</title>
         <input type="dropdown" token="Panel22Function1" id="Input_Panel22Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <input type="dropdown" token="Panel22Field1" id="Input_Panel22Field1" searchWhenChanged="true">
            <choice value="ProcNetKBPS">Network thruput (KB/s)</choice>
            <choice value="ProcGpuComputePercent">GPU compute (%)</choice>
            <choice value="ProcGpuMemMB">GPU memory (MB)</choice>
            <choice value="ProcHandleCount">Handle count</choice>
            <choice value="ProcThreadCount">Thread count</choice>
            <choice value="ProcPrivateMB">RAM - Private (MB)</choice>
            <choice value="ProcVirtualSizeMB">RAM - Virtual (MB)</choice>
            <choice value="ProcPageFaultsPS">Page faults (per s)</choice>
            <choice value="ProcPageFileMB">Page file (MB)</choice>
            <default>ProcNetKBPS</default>
            <label>Field:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart id="Chart_Panel22">
            <search base="Search_Charts">
               <query>
                  sort limit=10 -"$Panel22Function1$($Panel22Field1$)" 
                  | fields "$ModeGroupByAs$" "$Panel22Function1$($Panel22Field1$)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>
   </row>

   <!-- Row 3 -->
   <row>
      <panel>
         <title>Data table</title>
         <input type="dropdown" token="Panel31Function1" id="Input_Panel31Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <input type="multiselect" token="FieldFilter" id="Input_Panel31FieldFilter" searchWhenChanged="true">
            <choice value='"Total CPU time (s)"'>Total CPU time (s)</choice>
            <choice value='"CPU (%)"'>CPU (%)</choice>
            <choice value='"IOPS"'>IOPS</choice>
            <choice value='"Total IO (count)"'>Total IO (count)</choice>
            <choice value='"IO (MB/s)"'>IO (MB/s)</choice>
            <choice value='"Total IO (MB)"'>Total IO (MB)</choice>
            <choice value='"Avg. IO latency (ms)"'>Avg. IO latency (ms)</choice>
            <choice value='"RAM (MB)"'>RAM (MB)</choice>
            <choice value='"Net thruput (KB/s)"'>Net thruput (KB/s)</choice>
            <choice value='"Avg. net latency (ms)"'>Avg. net latency (ms)</choice>
            <choice value='"Handle count"'>Handle count</choice>
            <choice value='"Thread count"'>Thread count</choice>
            <choice value='"RAM - Private (MB)"'>RAM - Private (MB)</choice>
            <choice value='"RAM - Virtual (MB)"'>RAM - Virtual (MB)</choice>
            <choice value='"Page faults (per s)"'>Page faults (per s)</choice>
            <choice value='"Page file (MB)"'>Page file (MB)</choice>
            <choice value='"Process priority (latest)"'>Process priority (latest)</choice>
            <default>
               "Total CPU time (s)",
               "CPU (%)",
               "Total IO (count)",
               "IO (MB/s)",
               "Avg. IO latency (ms)",
               "RAM (MB)",
               "Avg. net latency (ms)",
               "Handle count",
               "Thread count"
            </default>
            <label>Fields to display:</label>
            <prefix></prefix>
            <suffix></suffix>
            <valuePrefix></valuePrefix>
            <valueSuffix></valueSuffix>
            <delimiter> </delimiter>
         </input>
         <table id="Table_Panel31">
            <search id="Search_TablePanel31">
               <query>
               </query>
            </search>
            <option name="count">50</option>
            <option name="drilldown">row</option>
         </table>

         <html src="process_detail_explanation.html">
         </html>
      </panel>
   </row>

</form>
