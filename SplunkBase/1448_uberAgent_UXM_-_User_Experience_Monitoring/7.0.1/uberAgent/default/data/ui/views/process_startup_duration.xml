   <!-- Row 4 -->
   <row depends="$DrilldownFilterBy31$">
      <panel>
         <title>Startup detail: $Panel31GroupByDisplay$ = $DrilldownFilterBy31$</title>
         <chart>
            <search>
               <query>
                  | pivot `uA_DM_Process_ProcessStartup` Process_ProcessStartup
                     sum(StartupIOCount) as SumStartupIOCount
                     sum(StartupTimeS) as SumStartupTimeS
                     count(Process_ProcessStartup) as CountProcessStartup
