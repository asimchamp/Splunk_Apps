   <!-- Row 3 -->
   <row>
      <panel>
         <title>Startup duration grouped</title>
         <input type="dropdown" token="Panel31GroupBy" searchWhenChanged="true">
            <choice value="AppName">Application</choice>
            <choice value="ProcName">Process</choice>
            <choice value="host">Host</choice>
            <choice value="ProcUser">User</choice>
            <default>$ModeGroupBy$</default>
            <label>Group by:</label>
            <change>
               <condition value="AppName">
                  <set token="Panel31GroupByDisplay">$label$</set>
                  <set token="Panel31GroupByOther">ProcName</set>
                  <set token="Panel31GroupByOtherDisplay">Process name(s)</set>
                  <set token="Panel31Splitrow">
                     splitrow AppName
                  </set>
                  <unset token="DrilldownFilterBy31"></unset>
               </condition>
               <condition value="ProcName">
                  <set token="Panel31GroupByDisplay">$label$</set>
                  <set token="Panel31GroupByOther">AppName</set>
                  <set token="Panel31GroupByOtherDisplay">App name(s)</set>
                  <set token="Panel31Splitrow"> </set>
                  <unset token="DrilldownFilterBy31"></unset>
               </condition>
               <condition value="host">
                  <set token="Panel31GroupByDisplay">$label$</set>
                  <set token="Panel31GroupByOther">$ModeGroupBy$</set>
                  <set token="Panel31GroupByOtherDisplay">$ModeGroupByAs$</set>
                  <set token="Panel31Splitrow">
                     splitrow host
                  </set>
                  <unset token="DrilldownFilterBy31"></unset>
               </condition>
               <condition value="ProcUser">
                  <set token="Panel31GroupByDisplay">$label$</set>
                  <set token="Panel31GroupByOther">$ModeGroupBy$</set>
                  <set token="Panel31GroupByOtherDisplay">$ModeGroupByAs$</set>
                  <set token="Panel31Splitrow">
                     splitrow ProcUser
                  </set>
                  <unset token="DrilldownFilterBy31"></unset>
               </condition>
            </change>
         </input>
         <input type="dropdown" token="Panel31Function1" id="Input_Panel31Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="max">maximum</choice>
            <choice value="min">minimum</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
            <change>
               <condition value="avg">
                  <set token="Panel31Function1Display">Avg.</set>
               </condition>
               <condition value="count">
                  <set token="Panel31Function1Display">Count</set>
               </condition>
               <condition value="max">
                  <set token="Panel31Function1Display">Max.</set>
               </condition>
               <condition value="min">
                  <set token="Panel31Function1Display">Min.</set>
               </condition>
               <condition value="sum">
                  <set token="Panel31Function1Display">Sum</set>
               </condition>
            </change>
         </input>
         <input type="radio" token="Panel31NewProcessesRadioButton">
            <label>$RadioButtonLabel$</label>
            <choice value="1">Yes</choice>
            <choice value="0">No</choice>
            <default>No</default>
            <change>
            <condition label="Yes">
               <set token="Panel31_OnlyNew">
                  | eval ProcNameLower = lower(ProcName)
                  | lookup lookup_processstartup_processlist ProcName as ProcNameLower OUTPUTNEW FirstSeen
                  | where isNull(FirstSeen) OR (EventTime-FirstSeen &lt; 3600)
               </set>
            </condition>
            <condition label="No">
               <set token="Panel31_OnlyNew"> </set>
            </condition>
            </change> 
         </input>
         <table id="Table_Panel31">
            <search>
               <query>
                  | pivot `uA_DM_Process_ProcessStartup` Process_ProcessStartup
                     earliest(_time) as EventTime
                     max(StartupIOCount) as maxStartupIOCount
                     min(StartupIOCount) as minStartupIOCount
                     sum(StartupIOCount) as sumStartupIOCount
                     max(StartupTimeS) as maxStartupTimeS
                     min(StartupTimeS) as minStartupTimeS
                     sum(StartupTimeS) as sumStartupTimeS
                     count(Process_ProcessStartup) as countProcessStartup
                     values($Panel31GroupByOther$) as "$Panel31GroupByOtherDisplay$"
                     dc(host) as counthost
                     $Panel31Splitrow$
                     splitrow
                        ProcName
                     $SearchFilter$
                     `uberAgent_filter_AppNameIsNotOS`
                  | eval EventTime = round (strptime (EventTime, "%Y-%m-%dT%H:%M:%S.%Q%z"), 0) 
                  $Panel31_OnlyNew$
                  | stats
                     max(maxStartupIOCount) as maxStartupIOCount
                     min(minStartupIOCount) as minStartupIOCount
                     sum(sumStartupIOCount) as sumStartupIOCount
                     max(maxStartupTimeS) as maxStartupTimeS
                     min(minStartupTimeS) as minStartupTimeS
                     sum(sumStartupTimeS) as sumStartupTimeS
                     sum(countProcessStartup) as countProcessStartup
                     values("$Panel31GroupByOtherDisplay$") as "$Panel31GroupByOtherDisplay$"
                     first(counthost) as "#Hosts"
                     by
                        $Panel31GroupBy$
                  | eval "$Panel31Function1Display$ startup IO count" = round (if ("$Panel31Function1$" = "avg", sumStartupIOCount / countProcessStartup, '$Panel31Function1$StartupIOCount') , 0)
                  | eval "$Panel31Function1Display$ startup duration (s)"= round (if ("$Panel31Function1$" = "avg", sumStartupTimeS / countProcessStartup, '$Panel31Function1$StartupTimeS') , 1)
                  | rename countProcessStartup as "#Startups"
                  | eval sortfield = lower('$Panel31GroupBy$')
                  | sort limit=0 sortfield
                  | rename $Panel31GroupBy$ as $Panel31GroupByDisplay$
                  | table
                     "$Panel31GroupByDisplay$"
                     "$Panel31GroupByOtherDisplay$"
                     "$Panel31Function1Display$ startup duration (s)"
                     "$Panel31Function1Display$ startup IO count"
                     "#Startups"
                     "#Hosts"
               </query>
            </search>
            <option name="count">20</option>
            <option name="drilldown">row</option>
            <drilldown>
               <set token="DrilldownFilterBy31">$click.value$</set>
            </drilldown>
         </table>

