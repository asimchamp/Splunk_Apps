            <option name="charting.chart">column</option>
            <option name="charting.legend.placement">bottom</option>
            <option name="charting.axisY2.enabled">true</option>
            <option name="charting.axisY2.fields">"Avg. startup IO count"</option>
            <option name="charting.axisY.includeZero">true</option>
            <option name="charting.axisY2.includeZero">true</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.text">sec.</option>
            <option name="charting.axisTitleY2.text">count</option>
            <option name="charting.drilldown">none</option>
         </chart>

         <input type="multiselect" token="FieldFilter" id="Input_Panel41FieldFilter" searchWhenChanged="true">
            <choice value='"User name"'>User name</choice>
            <choice value='"App name"'>App name</choice>
            <choice value='"App version"'>App version</choice>
            <choice value='"Process name"'>Process name</choice>
            <choice value='"Startup duration (s)"'>Startup duration (s)</choice>
            <choice value='"Startup IO count"'>Startup IO count</choice>
            <choice value='"Is elevated?"'>Is elevated?</choice>
            <choice value='"Is protected?"'>Is protected?</choice>
            <choice value='"HashMD5"'>HashMD5</choice>
            <choice value='"HashSHA1"'>HashSHA1</choice>
            <choice value='"HashSHA256"'>HashSHA256</choice>
            <choice value='"HashIMP"'>HashIMP</choice>
            <choice value='"Signature status"'>Signature status</choice>
            <choice value='"Is signed by OS vendor?"'>Is signed by OS vendor?</choice>
            <choice value='"Signer name?"'>Signer name?</choice>
            <default>
               "User name",
               "App name",
               "App version",
               "Process name",
               "Startup duration (s)",
               "Startup IO count",
               "Is elevated?",
               "Is protected?"
            </default>
            <label>Fields to display:</label>
            <prefix></prefix>
            <suffix></suffix>
            <valuePrefix></valuePrefix>
            <valueSuffix></valueSuffix>
            <delimiter> </delimiter>
         </input>
         <table id="Table_Panel41">
            <search>
               <query>
                  | pivot `uA_DM_Process_ProcessStartup` Process_ProcessStartup
                     latest(AppName) as "App name"
                     latest(AppVersion) as "App version"
                     latest(StartupIOCount) as "Startup IO count"
                     latest(_time) as StartTime
                     latest(IsElevated) as "Is elevated?"
                     latest(IsProtected) as "Is protected?"
                     latest(HashMD5) as HashMD5
                     latest(HashSHA1) as HashSHA1
                     latest(HashSHA256) as HashSHA256
                     latest(HashIMP) as HashIMP
                     latest(SignatureStatusDisplayName) as "Signature status"
                     latest(IsSignedByOSVendor) as "Is signed by OS vendor?"
                     latest(SignerName) as "Signer name?"
                     $SearchFilter$
                     filter $Panel31GroupBy$ is $DrilldownFilterBy31|s$
                     splitrow ProcName as "Process name"
                     splitrow StartupTimeS
                     splitrow host as Host
                     splitrow ProcUser as "User name"
                  | eval "Start time" = strftime (strptime (StartTime, "%Y-%m-%dT%H:%M:%S.%Q%z"), "%Y-%m-%d %H:%M:%S")
                  | eval "Startup duration (s)" = round (StartupTimeS, 2)
                  | table
                     "Start time"
                     "Host"
                     $FieldFilter$
                  | sort -"Start time"
               </query>
            </search>
            <option name="count">50</option>
            <option name="drilldown">none</option>
         </table>
      </panel>
   </row>

</form>
