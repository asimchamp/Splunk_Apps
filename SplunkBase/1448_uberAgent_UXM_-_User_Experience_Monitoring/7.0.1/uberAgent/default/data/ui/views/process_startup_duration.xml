                  | eval ProcNameLower = lower(ProcName)
                  | lookup lookup_processstartup_processlist ProcName as ProcNameLower OUTPUTNEW FirstSeen
                  | where isNull(FirstSeen) OR (EventTime-FirstSeen &lt; 3600)
