<?xml version='1.0' encoding='utf-8'?>
<form version="1.1" script="main.js, process_startup_duration.js" stylesheet="main.css, process_startup_duration.css">

   <label>Process Startup</label>
   <description>This dashboard displays detailed information about process startup performance.</description>

   <fieldset autoRun="true">
      <input type="time" searchWhenChanged="true">
         <label>Time range:</label>
         <default>
            <earliest>-1h</earliest>
            <latest>now</latest>
         </default>
         <change>
            <eval token="TimeRangePickerSpan">relative_time(now(), latest) - relative_time(now(), earliest)</eval>
            <eval token="PivotPeriodAutoSolo">case('TimeRangePickerSpan' &lt; 350000, "auto", 'TimeRangePickerSpan' &lt; 610000, "hour", 1=1, "auto")</eval>
            <eval token="PivotPeriodAutoTimechart">case('TimeRangePickerSpan' &lt; 1200, "second", 'TimeRangePickerSpan' &lt; 300000, "minute", 'TimeRangePickerSpan' &lt; 700000, "hour", 1=1, "day")</eval>
         </change>
      </input>
      <input type="dropdown" token="FilterField" id="Input_FilterField">
         <label>Filter field:</label>
      </input>
      <input type="dropdown" token="FilterOperator" id="Input_FilterOperator">
         <label>Filter operator:</label>
      </input>
      <input type="text" token="FilterExpression" id="Input_FilterExpression">
         <label>Filter expression:</label>
         <default>*</default>
      </input>
      <input type="dropdown" token="FilterField2" id="Input_FilterField2" depends="$FilterLevel2$">
      </input>
      <input type="dropdown" token="FilterOperator2" id="Input_FilterOperator2" depends="$FilterLevel2$">
      </input>
      <input type="text" token="FilterExpression2" id="Input_FilterExpression2" depends="$FilterLevel2$">
      </input>
      <input type="dropdown" token="FilterField3" id="Input_FilterField3" depends="$FilterLevel3$">
      </input>
      <input type="dropdown" token="FilterOperator3" id="Input_FilterOperator3" depends="$FilterLevel3$">
      </input>
      <input type="text" token="FilterExpression3" id="Input_FilterExpression3" depends="$FilterLevel3$">
      </input>
      <input type="dropdown" token="FilterField4" id="Input_FilterField4" depends="$FilterLevel4$">
      </input>
      <input type="dropdown" token="FilterOperator4" id="Input_FilterOperator4" depends="$FilterLevel4$">
      </input>
      <input type="text" token="FilterExpression4" id="Input_FilterExpression4" depends="$FilterLevel4$">
      </input>
      <input type="dropdown" token="FilterField5" id="Input_FilterField5" depends="$FilterLevel5$">
      </input>
      <input type="dropdown" token="FilterOperator5" id="Input_FilterOperator5" depends="$FilterLevel5$">
      </input>
      <input type="text" token="FilterExpression5" id="Input_FilterExpression5" depends="$FilterLevel5$">
      </input>
      <input type="dropdown" token="FilterField6" id="Input_FilterField6" depends="$FilterLevel6$">
      </input>
      <input type="dropdown" token="FilterOperator6" id="Input_FilterOperator6" depends="$FilterLevel6$">
      </input>
      <input type="text" token="FilterExpression6" id="Input_FilterExpression6" depends="$FilterLevel6$">
      </input>
      <input type="text" token="FilterLevels" id="Input_FilterLevels" depends="$FilterLevels_Hidden$">
      </input>
      <input type="text" token="FilterDatamodel" id="Input_FilterDatamodel" depends="$FilterDatamodel_Hidden$">
         <default>Process_ProcessStartup</default>
      </input>
   </fieldset>
   
   <search id="Search_Single">
      <query>
         | pivot `uA_DM_Process_ProcessStartup` Process_ProcessStartup
            sum(StartupIOCount) as SumStartupIOCount
            sum(StartupTimeS) as SumStartupTimeS
            count(Process_ProcessStartup) as CountProcessStartup
            splitrow
               $ModeGroupBy$
            $SearchFilter$
            `uberAgent_filter_AppNameIsNotOS`
         | stats
            sum(SumStartupIOCount) as SumStartupIOCount
            sum(SumStartupTimeS) as SumStartupTimeS
            sum(CountProcessStartup) as CountProcessStartup
            dc($ModeGroupBy$) as UniqueProcesses
         | eval AvgStartupIOCount = round (SumStartupIOCount / CountProcessStartup,1)
         | eval AvgStartupDurationS = round (SumStartupTimeS / CountProcessStartup,1)
      </query>
   </search>
   
   <search id="Search_New">
      <query>
         | pivot `uA_DM_Process_ProcessStartup` Process_ProcessStartup
            earliest(_time) as EventTime
            sum(StartupIOCount) as SumStartupIOCount
            sum(StartupTimeS) as SumStartupTimeS
            count(Process_ProcessStartup) as CountProcessStartup
            splitrow
               ProcName
            splitrow
               AppName
            $SearchFilter$
            `uberAgent_filter_AppNameIsNotOS`
         | eval EventTime = round (strptime (EventTime, "%Y-%m-%dT%H:%M:%S.%Q%z"), 0) 
         | eval ProcNameLower = lower(ProcName)
         | lookup lookup_processstartup_processlist ProcName as ProcNameLower OUTPUTNEW FirstSeen
         | where isNull(FirstSeen) OR (EventTime-FirstSeen &lt; 3600)
         | stats
            sum(SumStartupIOCount) as SumStartupIOCount
            sum(SumStartupTimeS) as SumStartupTimeS
            sum(CountProcessStartup) as CountProcessStartup
            by
               $ModeGroupBy$
         | stats
            sum(SumStartupIOCount) as SumStartupIOCount
            sum(SumStartupTimeS) as SumStartupTimeS
            sum(CountProcessStartup) as CountProcessStartup
            dc($ModeGroupBy$) as UniqueProcessesOrApps
         | eval AvgStartupIOCount = round (SumStartupIOCount / CountProcessStartup,1)
         | eval AvgStartupDurationS = round (SumStartupTimeS / CountProcessStartup,1)
      </query>
   </search>
   
   <!-- Row 1 -->
   <row>      
      <!-- Column 1 -->
      <panel>
         <title>Overview</title>
         <viz type="uberAgent.uberagent-singlevalue">
            <search base="Search_Single" />
            <option name="uberAgent.uberagent-singlevalue.1-field">UniqueProcesses</option>
            <option name="uberAgent.uberagent-singlevalue.1-title">Unique $ModeGroupByPluralLower$:</option>
            <option name="uberAgent.uberagent-singlevalue.2-field">CountProcessStartup</option>
            <option name="uberAgent.uberagent-singlevalue.2-title">Total process starts:</option>
            <option name="uberAgent.uberagent-singlevalue.3-field">AvgStartupDurationS</option>
            <option name="uberAgent.uberagent-singlevalue.3-title">Avg. startup duration:</option>
            <option name="uberAgent.uberagent-singlevalue.3-afterLabel">s</option>
            <option name="uberAgent.uberagent-singlevalue.4-field">AvgStartupIOCount</option>
            <option name="uberAgent.uberagent-singlevalue.4-title">Avg. startup IO count:</option>
         </viz>
      </panel>
      
      <!-- Column 2 -->
      <panel>
         <title>Overview - new $ModeGroupByPluralLower$</title>
         <viz type="uberAgent.uberagent-singlevalue">
            <search base="Search_New" />
            <option name="uberAgent.uberagent-singlevalue.1-field">UniqueProcessesOrApps</option>
            <option name="uberAgent.uberagent-singlevalue.1-title">Unique new $ModeGroupByPluralLower$:</option>
            <option name="uberAgent.uberagent-singlevalue.2-field">CountProcessStartup</option>
            <option name="uberAgent.uberagent-singlevalue.2-title">Total new process starts:</option>
            <option name="uberAgent.uberagent-singlevalue.3-field">AvgStartupDurationS</option>
            <option name="uberAgent.uberagent-singlevalue.3-title">Avg. startup duration:</option>
            <option name="uberAgent.uberagent-singlevalue.3-afterLabel">s</option>
            <option name="uberAgent.uberagent-singlevalue.4-field">AvgStartupIOCount</option>
            <option name="uberAgent.uberagent-singlevalue.4-title">Avg. startup IO count:</option>
         </viz>
      </panel>
   </row>

