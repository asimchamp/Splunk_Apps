         <chart id="Chart_Panel21">
            <search>
               <query>
                  | pivot `uA_DM_Process_ProcessStartup` Process_ProcessStartup
                     earliest(_time) as EventTime
                     max(StartupTimeS) as maxStartupTimeS
                     min(StartupTimeS) as minStartupTimeS
                     sum(StartupTimeS) as sumStartupTimeS
                     count(Process_ProcessStartup) as countProcessStartup
                     $Panel21Splitrow$
                     splitrow
                        ProcName
                     $SearchFilter$
                     `uberAgent_filter_AppNameIsNotOS`
                  | eval EventTime = round (strptime (EventTime, "%Y-%m-%dT%H:%M:%S.%Q%z"), 0) 
                  $Panel21_OnlyNew$
                  | stats
                     max(maxStartupTimeS) as maxStartupTimeS
                     min(minStartupTimeS) as minStartupTimeS
                     sum(sumStartupTimeS) as sumStartupTimeS
                     sum(countProcessStartup) as countProcessStartup
                     by
                        $Panel21GroupBy$
                  | eval "$Panel21Function1$(StartupTimeS)" = if ("$Panel21Function1$" = "avg", round (sumStartupTimeS / countProcessStartup, 1), '$Panel21Function1$StartupTimeS')
                  | sort limit=10 - "$Panel21Function1$(StartupTimeS)"
                  | rename $Panel21GroupBy$ as $Panel21GroupByDisplay$
                  | table 
                     $Panel21GroupByDisplay$
                     "$Panel21Function1$(StartupTimeS)"
               </query>

            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>

