<?xml version='1.0' encoding='utf-8'?>
<form version="1.1" script="main.js" stylesheet="main.css, citrixadc_performance.css">

   <label>Citrix ADC Appliance Performance</label>
   <description>This dashboard displays performance information about Citrix ADC appliances monitored with uberAgent. uberAgent only collects performance data from the primary Citrix ADC.</description>

   <fieldset autoRun="true">
    <input type="time" searchWhenChanged="true">
      <label>Time range:</label>
      <default>
        <earliest>-1h</earliest>
        <latest>now</latest>
      </default>
      <change>
         <eval token="TimeRangePickerSpan">relative_time(now(), latest) - relative_time(now(), earliest)</eval>
         <eval token="PivotPeriodAutoSolo">case('TimeRangePickerSpan' &lt; 350000, "auto", 'TimeRangePickerSpan' &lt; 610000, "hour", 1=1, "auto")</eval>
         <eval token="PivotPeriodAutoTimechart">case('TimeRangePickerSpan' &lt; 1200, "second", 'TimeRangePickerSpan' &lt; 300000, "minute", 'TimeRangePickerSpan' &lt; 700000, "hour", 1=1, "day")</eval>
      </change>
    </input>
    <input type="dropdown" token="CitrixADCFilterString" searchWhenChanged="true">
         <choice value="*">All</choice>
         <default>*</default>
         <label>Citrix ADC:</label>
         <fieldForLabel>HostName</fieldForLabel>
         <fieldForValue>HostName</fieldForValue>
         <allowCustomValues>false</allowCustomValues>
         <search>
            <query>
               | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                  splitrow HostName
               | eval sortfield = lower (HostName)
               | sort sortfield
               | table
                  HostName
            </query>
         </search>
    </input>
  </fieldset>
   
   <search id="Search_Single">
      <query>
         | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
            avg(PktCpuUsagePcnt) as PktCpuUsagePcnt
            avg(MgmtCpuUsagePcnt) as MgmtCpuUsagePcnt
            latest(NumCpus) as NumCpus
            avg(MemUsagePcnt) as MemUsagePcnt
            avg(Disk0PerUsage) as Disk0PerUsage
            avg(Disk1PerUsage) as Disk1PerUsage
            avg(ApplianceMBReceived) as ApplianceMBReceived
            avg(ApplianceMBSent) as ApplianceMBSent
         | eval CpuUsagePcnt = round(PktCpuUsagePcnt,1)
         | eval MgmtCpuUsagePcnt = round(MgmtCpuUsagePcnt,1)
         | eval MemUsagePcnt = round (MemUsagePcnt,1)
         | eval Disk0PerUsage = round(Disk0PerUsage,1)
         | eval Disk1PerUsage = round(Disk1PerUsage,1)
         | eval ApplianceThroughput = round(ApplianceMBReceived + ApplianceMBSent,1)
      </query>
   </search>
   
   <!-- Row 1 -->
   <row>
      <!-- Column 1 -->
      <panel>
        <title>Averages for all Citrix ADCs</title>
         <viz type="uberAgent.uberagent-singlevalue">
            <search base="Search_Single" />
            <option name="uberAgent.uberagent-singlevalue.1-field">CpuUsagePcnt</option>
            <option name="uberAgent.uberagent-singlevalue.1-title">Avg. packet CPU usage:</option>
            <option name="uberAgent.uberagent-singlevalue.1-afterLabel">%</option>
            <option name="uberAgent.uberagent-singlevalue.2-field">MgmtCpuUsagePcnt</option>
            <option name="uberAgent.uberagent-singlevalue.2-title">Avg. mgmt. CPU usage:</option>
            <option name="uberAgent.uberagent-singlevalue.2-afterLabel">%</option>
            <option name="uberAgent.uberagent-singlevalue.3-field">MemUsagePcnt</option>
            <option name="uberAgent.uberagent-singlevalue.3-title">Avg. RAM usage:</option>
            <option name="uberAgent.uberagent-singlevalue.3-afterLabel">%</option>
            <option name="uberAgent.uberagent-singlevalue.4-field">Disk0PerUsage</option>
            <option name="uberAgent.uberagent-singlevalue.4-title">Avg. /flash usage:</option>
            <option name="uberAgent.uberagent-singlevalue.4-afterLabel">%</option>
            <option name="uberAgent.uberagent-singlevalue.5-field">Disk1PerUsage</option>
            <option name="uberAgent.uberagent-singlevalue.5-title">Avg. /var usage:</option>
            <option name="uberAgent.uberagent-singlevalue.5-afterLabel">%</option>
            <option name="uberAgent.uberagent-singlevalue.6-field">ApplianceThroughput</option>
            <option name="uberAgent.uberagent-singlevalue.6-title">Avg. network data volume:</option>
            <option name="uberAgent.uberagent-singlevalue.6-afterLabel">MB</option>
         </viz>
      </panel>
   </row>
   
   <!-- Row 2 -->
   <row>
     <!-- Column 1 -->
     <panel>
         <title>CPU usage (%) per Citrix ADC (top 10)</title>
         <input type="dropdown" token="Panel21Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <input type="dropdown" token="Panel21Field1" id="Input_Panel21Field1" searchWhenChanged="true">
            <choice value="PktCpuUsagePcnt">Packet CPU usage (%)</choice>
            <choice value="MgmtCpuUsagePcnt">Mgmt. CPU usage (%)</choice>
            <default>PktCpuUsagePcnt</default>
            <label>Field:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     $Panel21Function1$(PktCpuUsagePcnt) as PktCpuUsagePcnt
                     $Panel21Function1$(MgmtCpuUsagePcnt) as MgmtCpuUsagePcnt
                     latest(NumCpus) as NumCpus
                     splitrow
                        HostName
                     filter HostName is $CitrixADCFilterString|s$
                  | eval "$Panel21Function1$($Panel21Field1$)" = round($Panel21Field1$,1)
                  | sort limit=10 - "$Panel21Function1$($Panel21Field1$)"
                  | table
                     HostName
                     "$Panel21Function1$($Panel21Field1$)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>
      
      <!-- Column 2 -->
      <panel>
         <title>RAM usage (%) per Citrix ADC (top 10)</title>
         <input type="dropdown" token="Panel22Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     $Panel22Function1$(MemUsagePcnt) as "$Panel22Function1$(MemUsagePcnt)"
                     splitrow
                        HostName
                     filter HostName is $CitrixADCFilterString|s$
                  | eval "$Panel22Function1$(MemUsagePcnt)" = round('$Panel22Function1$(MemUsagePcnt)',1)
                  | sort limit=10 - "$Panel22Function1$(MemUsagePcnt)"
                  | table
                     HostName
                     "$Panel22Function1$(MemUsagePcnt)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
      </panel>
   </row>
   
      <!-- Row 3 -->
   <row>
     <!-- Column 1 -->
     <panel>
         <title>Disk usage (%) per Citrix ADC (top 10)</title>
         <input type="dropdown" token="Panel31Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <input type="dropdown" token="Panel31Field1" id="Input_Panel31Field1" searchWhenChanged="true">
            <choice value="Disk0PerUsage">/flash usage (%)</choice>
            <choice value="Disk1PerUsage">/var usage (%)</choice>
            <default>Disk0PerUsage</default>
            <label>Field:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     $Panel31Function1$($Panel31Field1$) as "$Panel31Function1$($Panel31Field1$)"
                     splitrow
                        HostName
                     filter HostName is $CitrixADCFilterString|s$
                  | eval "$Panel31Function1$($Panel31Field1$)" = round('$Panel31Function1$($Panel31Field1$)',1)
                  | sort limit=10 - "$Panel31Function1$($Panel31Field1$)"
                  | table
                     HostName
                     "$Panel31Function1$($Panel31Field1$)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
         </chart>
         
      </panel>
      
      <!-- Column 2 -->
     <panel>
         <title>Network data volume (MB) per Citrix ADC (top 10)</title>
         <input type="dropdown" token="Panel32Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>sum</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
         </input>
         <chart>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     $Panel32Function1$(ApplianceMBReceived) as "$Panel32Function1$(ApplianceMBReceived)"
                     $Panel32Function1$(ApplianceMBSent) as "$Panel32Function1$(ApplianceMBSent)"
                     splitrow
                        HostName
                     filter HostName is $CitrixADCFilterString|s$
                  | eval "Receive volume (MB)" = round('$Panel32Function1$(ApplianceMBReceived)',1)
                  | eval "Send volume (MB)" = round('$Panel32Function1$(ApplianceMBSent)',1)
                  | eval "Send+Receive volume (MB)" = 'Receive volume (MB)' + 'Send volume (MB)'
                  | sort limit=10 - "Send+Receive volume (MB)"
                  | table
                     HostName
                     "Send volume (MB)"
                     "Receive volume (MB)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">bar</option>
            <option name="charting.chart.stackMode">stacked</option>
            <option name="charting.legend.placement">none</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.visibility">collapsed</option>
         </chart>
         
      </panel>
   </row>

   <!-- Row 4 -->   
   <row>
      <panel>
         <title>Data table</title>
         <input type="dropdown" token="Panel41Function1" searchWhenChanged="true">
            <choice value="avg">average</choice>
            <choice value="count">count</choice>
            <choice value="max">maximum</choice>
            <choice value="median">median</choice>
            <choice value="min">minimum</choice>
            <choice value="stdev">standard deviation</choice>
            <choice value="sum">sum</choice>
            <default>avg</default>
            <label>Function:</label>
            <prefix></prefix>
            <suffix></suffix>
            <change>
               <condition value="avg">
                  <set token="Panel41Function1Display">Avg.</set>
               </condition>
               <condition value="count">
                  <set token="Panel41Function1Display">Count</set>
               </condition>
               <condition value="max">
                  <set token="Panel41Function1Display">Max.</set>
               </condition>
               <condition value="median">
                  <set token="Panel41Function1Display">Med.</set>
               </condition>
               <condition value="min">
                  <set token="Panel41Function1Display">Min.</set>
               </condition>
               <condition value="stdev">
                  <set token="Panel41Function1Display">Stdev</set>
               </condition>
               <condition value="sum">
                  <set token="Panel41Function1Display">Sum</set>
               </condition>
            </change>
         </input>
         <table>
            <search>
               <query>
                     | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                        $Panel41Function1$(PktCpuUsagePcnt) as "$Panel41Function1$(PktCpuUsagePcnt)"
                        $Panel41Function1$(MgmtCpuUsagePcnt) as "$Panel41Function1$(MgmtCpuUsagePcnt)"
                        $Panel41Function1$(MemUsagePcnt) as "$Panel41Function1$(MemUsagePcnt)"
                        $Panel41Function1$(Disk0PerUsage) as "$Panel41Function1$(Disk0PerUsage)"
                        $Panel41Function1$(Disk1PerUsage) as "$Panel41Function1$(Disk1PerUsage)"
                        $Panel41Function1$(ApplianceMBReceived) as "$Panel41Function1$(ApplianceMBReceived)"
                        $Panel41Function1$(ApplianceMBSent) as "$Panel41Function1$(ApplianceMBSent)"
                        filter HostName is $CitrixADCFilterString|s$
                        splitrow
                           HostName as "Host name"
                     | eval "$Panel41Function1Display$ packet CPU usage (%)" = round('$Panel41Function1$(PktCpuUsagePcnt)',1)
                     | eval "$Panel41Function1Display$ mgmt. CPU usage (%)" = round('$Panel41Function1$(MgmtCpuUsagePcnt)',1)
                     | eval "$Panel41Function1Display$ RAM usage (%)" = round ('$Panel41Function1$(MemUsagePcnt)',1)
                     | eval "$Panel41Function1Display$ /flash usage (%)" = round('$Panel41Function1$(Disk0PerUsage)',1)
                     | eval "$Panel41Function1Display$ /var usage (%)" = round('$Panel41Function1$(Disk1PerUsage)',1)
                     | eval "$Panel41Function1Display$ network data volume (MB)" = round('$Panel41Function1$(ApplianceMBReceived)' + '$Panel41Function1$(ApplianceMBSent)',1)
                     | eval sortfield=lower('Host name')
                     | sort limit=0 sortfield
                     | table
                      "Host name"
                      "$Panel41Function1Display$ packet CPU usage (%)"
                      "$Panel41Function1Display$ mgmt. CPU usage (%)"
                      "$Panel41Function1Display$ RAM usage (%)"
                      "$Panel41Function1Display$ /flash usage (%)"
                      "$Panel41Function1Display$ /var usage (%)"
                      "$Panel41Function1Display$ network data volume (MB)"
               </query>
            </search>
            <option name="count">10</option>
            <option name="drilldown">row</option>
            <drilldown>
                <set token="DrilldownFilterBy51">$click.value$</set>
            </drilldown>
         </table>
         <html src="citrixadc_performance_explanation.html">
         </html>
          <chart depends="$DrilldownFilterBy51$">
            <title>$DrilldownFilterBy51$: CPU usage over time</title>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     latest(MgmtCpuUsagePcnt) as "Mgmt. CPU usage (%)"
                     latest(PktCpuUsagePcnt) as "Packet CPU usage (%)"
                     latest(NumCpus) as NumCpus
                     splitrow
                        _time 
                        period $PivotPeriodAutoSolo$
                     filter HostName is $CitrixADCFilterString|s$
                     filter HostName is $DrilldownFilterBy51|s$
                  | eval "Mgmt. CPU usage (%)" = round('Mgmt. CPU usage (%)',1)
                  | eval "Packet CPU usage (%)" = round('Packet CPU usage (%)',1)
                  | table
                    _time
                    "Packet CPU usage (%)"
                    "Mgmt. CPU usage (%)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">line</option>
            <option name="charting.legend.placement">right</option>
            <option name="charting.axisY.includeZero">true</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.visibility">collapsed</option>
            <option name="charting.chart.nullValueMode">connect</option>
            <option name="charting.drilldown">none</option>
         </chart>
            <chart depends="$DrilldownFilterBy51$">
            <title>$DrilldownFilterBy51$: RAM usage over time</title>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     latest(MemUsagePcnt) as "RAM usage (%)"
                     splitrow
                        _time 
                        period $PivotPeriodAutoSolo$
                    filter HostName is $CitrixADCFilterString|s$
                    filter HostName is $DrilldownFilterBy51|s$
                  | table
                    _time
                    "RAM usage (%)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">line</option>
            <option name="charting.legend.placement">right</option>
            <option name="charting.axisY.includeZero">true</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.visibility">collapsed</option>
            <option name="charting.chart.nullValueMode">connect</option>
            <option name="charting.drilldown">none</option>
         </chart>
         <chart depends="$DrilldownFilterBy51$">
            <title>$DrilldownFilterBy51$: Disk usage over time</title>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     latest(Disk0PerUsage) as "/flash usage (%)"
                     latest(Disk1PerUsage) as "/var usage (%)"
                     splitrow
                        _time 
                        period $PivotPeriodAutoSolo$
                    filter HostName is $CitrixADCFilterString|s$
                    filter HostName is $DrilldownFilterBy51|s$
                  | table
                    _time
                    "/flash usage (%)"
                    "/var usage (%)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">line</option>
            <option name="charting.legend.placement">right</option>
            <option name="charting.axisY.includeZero">true</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.visibility">collapsed</option>
            <option name="charting.chart.nullValueMode">connect</option>
            <option name="charting.drilldown">none</option>
         </chart>
         <chart depends="$DrilldownFilterBy51$">
            <title>$DrilldownFilterBy51$: Network data volume over time</title>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     sum(ApplianceMBReceived) as ApplianceMBReceived
                     sum(ApplianceMBSent) as ApplianceMBSent
                     splitrow
                        _time
                        period $PivotPeriodAutoTimechart$
                     filter HostName is $CitrixADCFilterString|s$
                     filter HostName is $DrilldownFilterBy51|s$
                  | eval ApplianceMBReceived = round(ApplianceMBReceived,1)
                  | eval ApplianceMBSent = round(ApplianceMBSent,1)
                  | stats 
                     avg(ApplianceMBReceived) as ApplianceMBReceived
                     avg(ApplianceMBSent) as ApplianceMBSent
                        by _time
                  | timechart
                     `uberAgent_dynamic_span`
                     sum(ApplianceMBReceived) as "Receive volume (MB)"
                     sum(ApplianceMBSent) as "Send volume (MB)"
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">line</option>
            <option name="charting.legend.placement">right</option>
            <option name="charting.axisY.includeZero">true</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.visibility">collapsed</option>
            <option name="charting.chart.nullValueMode">connect</option>
            <option name="charting.drilldown">none</option>
         </chart>
       </panel>
    </row>
    <row depends="$DrilldownFilterBy51$">
        <panel>
            <html>
              <h1>MPX only</h1>
            </html>
        </panel>
    </row>
    <row depends="$DrilldownFilterBy51$">
        <panel>
        <chart depends="$DrilldownFilterBy51$">
            <title>$DrilldownFilterBy51$: Temperature &amp; fan speed over time</title>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
