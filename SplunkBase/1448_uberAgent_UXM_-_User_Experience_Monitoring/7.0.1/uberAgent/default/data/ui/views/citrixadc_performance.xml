    </row>
    <row depends="$DrilldownFilterBy51$">
        <panel id="VoltageOverTimePanel">
         <chart depends="$DrilldownFilterBy51$">
            <title>$DrilldownFilterBy51$: Voltage over time</title>
            <search>
               <query>
                  | pivot `uA_DM_CitrixADC_AppliancePerformance` CitrixADC_AppliancePerformance
                     latest(VoltageV33Main) as "Voltage (V)"
                     splitrow
                        _time 
                        period $PivotPeriodAutoSolo$
                    filter HostName is $CitrixADCFilterString|s$
                    filter HostName is $DrilldownFilterBy51|s$
                    | eval "Upper limit (V)" = 3.630
                    | eval "Lower limit (V)" = 2.970
               </query>
            </search>
            <option name="height">250</option>
            <option name="charting.chart">line</option>
            <option name="charting.legend.placement">right</option>
            <option name="charting.axisY.includeZero">true</option>
            <option name="charting.axisTitleX.visibility">collapsed</option>
            <option name="charting.axisTitleY.visibility">collapsed</option>
            <option name="charting.chart.nullValueMode">connect</option>
            <option name="charting.drilldown">none</option>
            <option name="charting.fieldDashStyles">{"Upper limit (V)":"shortDash", "Lower limit (V)":"shortDash"}</option>
         </chart>
      </panel>
      <panel id="PowerSupply1Panel">
         <chart depends="$DrilldownFilterBy51$">
