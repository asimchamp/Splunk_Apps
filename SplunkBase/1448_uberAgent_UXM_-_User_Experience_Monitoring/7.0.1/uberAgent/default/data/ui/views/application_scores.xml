               <condition value="max">
                  <set token="Panel71Function1Display">Max.</set>
               </condition>
               <condition value="median">
                  <set token="Panel71Function1Display">Med.</set>
               </condition>
               <condition value="min">
                  <set token="Panel71Function1Display">Min.</set>
               </condition>
               <condition value="stdev">
                  <set token="Panel71Function1Display">Stdev</set>
               </condition>
               <condition value="sum">
                  <set token="Panel71Function1Display">Sum</set>
               </condition>
            </change>
         </input>
         <table>
            <search>
               <query>
                  | pivot `uA_DM_Score_Application` Score_Application
                     $Panel71Function1$(app_cpu) as app_cpu
                     $Panel71Function1$(app_ram) as app_ram
                     $Panel71Function1$(app_diskio) as app_diskio
                     $Panel71Function1$(app_networkavailability) as app_networkavailability
                     $Panel71Function1$(app_errors) as app_errors
                     $Panel71Function1$(app_uidelay) as app_uidelay
                     $Panel71Function1$(app_networklatency) as app_networklatency
                     $Panel71Function1$(app_score) as app_score
                     splitrow
                        AppName as Application
                     filter $Panel61GroupBy$ is $DrilldownFilterBy61|s$
                     $SearchFilter$
                     $AllowedIndexesFilter$
                  | eval "$Panel71Function1Display$ Application score" = round(app_score,1)
                  | eval "$Panel71Function1Display$ CPU score" = round(app_cpu,1)
                  | eval "$Panel71Function1Display$ RAM score" = round(app_ram,1)
                  | eval "$Panel71Function1Display$ Disk IO score" = round(app_diskio,1)
                  | eval "$Panel71Function1Display$ Network availability score" = round(app_networkavailability,1)
                  | eval "$Panel71Function1Display$ App errors score" = round(app_errors,1)
                  | eval "$Panel71Function1Display$ UI delay score" = round(app_uidelay,1)
                  | eval "$Panel71Function1Display$ Network latency score" = round(app_networklatency,1)
                  | eval sortfield=lower(Application)
                  | table
                     Application
                     "$Panel71Function1Display$ Application score"
                     "$Panel71Function1Display$ CPU score"
                     "$Panel71Function1Display$ RAM score"
                     "$Panel71Function1Display$ Disk IO score"
                     "$Panel71Function1Display$ Network availability score"
                     "$Panel71Function1Display$ App errors score"
                     "$Panel71Function1Display$ UI delay score"
                     "$Panel71Function1Display$ Network latency score"
                     sortfield
                  | sort limit=0 sortfield
                  | fields - sortfield
               </query>
            </search>
            <option name="count">50</option>
            <option name="drilldown">row</option>
            <format type="color" field="$Panel71Function1Display$ Application score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ CPU score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ RAM score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ Disk IO score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ Network availability score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ App errors score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ UI delay score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <format type="color" field="$Panel71Function1Display$ Network latency score">
               <colorPalette type="list">[#DC4E41,#F8BE34,#53A051]</colorPalette>
               <scale type="threshold">4,7</scale>
            </format>
            <drilldown>
               <link>
                  <![CDATA[
                     single_application_detail?earliest=$earliest$&latest=$latest$&form.FilterField=sAppName&form.FilterOperator=in&form.FilterExpression=$click.value|s$
                  ]]>
               </link>
            </drilldown>
         </table>
      </panel>
