                     $SearchFilter$
                     | eval "$Panel21Function1Display$ $ModeLower$ UI unresponsiveness (s) per host" = round(FuncUIDelayMs/1000,1)
                     | sort limit=10 - "$Panel21Function1Display$ $ModeLower$ UI unresponsiveness (s) per host"
                     | fields
