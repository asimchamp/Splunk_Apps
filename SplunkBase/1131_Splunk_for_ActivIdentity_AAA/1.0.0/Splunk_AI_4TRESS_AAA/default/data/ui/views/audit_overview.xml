<view template="dashboard.html" stylesheet="traffic_overview.css">
  <label>Audit Dashboard</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="navigationHeader" />
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="viewHeader">
    <param name="actionsMenuFilter">dashboard</param>
  </module>
  <module name="GenericHeader" layoutPanel="panel_row1_col1">
    <param name="label">Audit Dashboard</param>
  </module>
  <module name="StaticContentSample" layoutPanel="panel_row1_col1">
    <param name="text">This dashboard provides visibility into the audit data in your 4TRESS AAA environment.</param>
  </module>
  <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
    <param name="label">User ID:</param>
    <param name="field">userID</param>
    <param name="replacementMap">
      <param name="arg">
        <param name="userID">
          <param name="value"></param>
        </param>
      </param>
    </param>
    <param name="intention">
      <param name="name">stringreplace</param>
      <param name="arg">
        <param name="userID">
          <param name="default"></param>
          <param name="prefix">userID="</param>
          <param name="suffix">"</param>
          <param name="fillOnEmpty">false</param>
        </param>
      </param>
    </param>
    <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp2">
      <param name="label">Object:</param>
      <param name="field">object</param>
      <param name="replacementMap">
        <param name="arg">
          <param name="object">
            <param name="value"></param>
          </param>
        </param>
      </param>
      <param name="intention">
        <param name="name">stringreplace</param>
        <param name="arg">
          <param name="object">
            <param name="default"></param>
            <param name="prefix">object="</param>
            <param name="suffix">"</param>
            <param name="fillOnEmpty">false</param>
          </param>
        </param>
      </param>
      <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp2">
        <param name="label">Object Name:</param>
        <param name="field">object_name</param>
        <param name="replacementMap">
          <param name="arg">
            <param name="object_name">
              <param name="value"></param>
            </param>
          </param>
        </param>
        <param name="intention">
          <param name="name">stringreplace</param>
          <param name="arg">
            <param name="object_name">
              <param name="default"></param>
              <param name="prefix">object_name="</param>
              <param name="suffix">"</param>
              <param name="fillOnEmpty">false</param>
            </param>
          </param>
        </param>
        <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp3">
          <param name="label">Operation:</param>
          <param name="field">operation</param>
          <param name="replacementMap">
            <param name="arg">
              <param name="operation">
                <param name="value"></param>
              </param>
            </param>
          </param>
          <param name="intention">
            <param name="name">stringreplace</param>
            <param name="arg">
              <param name="operation">
                <param name="default"></param>
                <param name="prefix">operation="</param>
                <param name="suffix">"</param>
                <param name="fillOnEmpty">false</param>
              </param>
            </param>
          </param>
          <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
            <param name="label">Operation Detail:</param>
            <param name="field">operation_detail</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="operation_detail">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="operation_detail">
                  <param name="default"></param>
                  <param name="prefix">operation_detail="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>
              
		<module name="TimeRangePicker" layoutPanel="panel_row2_col1_grp1" autoRun="True">
                <param name="selected">Last 60 minutes</param>
                <param name="searchWhenChanged">True</param>
                <module name="HiddenSearch" layoutPanel="panel_row1_col1">
                  <param name="search">$object$ $userID$ $object_name$ $operation$ $operation_detail$ `ActivIdentity-Audit-DataCube`</param>
                  <module name="JobProgressIndicator"></module>
                  <module name="ViewRedirectorLink">
                    <param name="viewTarget">flashtimeline</param>
                    <param name="label">View Full Report</param>
                  </module>

                  <module name="HiddenPostProcess" layoutPanel="panel_row4_col1">
                    <param name="search">`AI-Audit-DataCube-Operation-Detail-by-Time`</param>
		    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Operation Detail Over The Time</param>
                      <param name="chart">line</param>
                      <param name="charting.legend.nullValueMode">connect</param>
                      <param name="charting.legend.showMarkers">true</param>
                      <param name="chart.stackMode">unstacked</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>                    
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

		 <module name="HiddenPostProcess" layoutPanel="panel_row5_col1">
                    <param name="search">`AI-Audit-DataCube-Operation-by-Time`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Operation Over Time</param>
                      <param name="chart">line</param>
                      <param name="charting.legend.nullValueMode">connect</param>
                      <param name="charting.legend.showMarkers">true</param>
                      <param name="chart.stackMode">unstacked</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

		<module name="HiddenPostProcess" layoutPanel="panel_row6_col1">
                    <param name="search">`AI-Audit-DataCube-User-by-Time`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Per User Action Over Time</param>
                      <param name="chart">line</param>
                      <param name="charting.legend.nullValueMode">connect</param>
                      <param name="charting.legend.showMarkers">true</param>
                      <param name="chart.stackMode">unstacked</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

                  <module name="HiddenPostProcess" layoutPanel="panel_row7_col1">
                    <param name="search">`AI-Audit-DataCube-Operation-by-Request`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top Operation</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

                  <module name="HiddenPostProcess" layoutPanel="panel_row7_col2">
                    <param name="search">`AI-Audit-DataCube-Operation-Detail-by-Request`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top Operation Detail</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

		<module name="HiddenPostProcess" layoutPanel="panel_row7_col3">
                    <param name="search">`AI-Audit-DataCube-User-by-Request`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top Users</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

                </module>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
</view>
