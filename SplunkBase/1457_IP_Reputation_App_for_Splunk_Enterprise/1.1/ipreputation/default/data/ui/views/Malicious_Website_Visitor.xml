<form>
  <label>Beta: Malicious Website Visitor</label>
  <fieldset submitButton="true">
    <input type="time" token="field1">
      <label></label>
      <default>
        <earliest>-60m@m</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <chart>
      <title>Threatscore AVG Last 7 Days</title>
      <search>
        <query>clientip=* | where threatscore&gt;0 | stats avg(threatscore) as threatscore_avg | eval threatscore_avg=round(threatscore_avg,2)</query>
        <earliest>-7d@d</earliest>
        <latest>-1d@d</latest>
      </search>
      <option name="charting.chart">fillerGauge</option>
      <option name="charting.chart.rangeValues">[0,3,5,10,20,40]</option>
      <option name="charting.gaugeColors">[0x84e900,0xffe800,0xf7912c,0xd85e3d,0xbf3030]</option>
      <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
      <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisTitleY2.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.axisY2.enabled">false</option>
      <option name="charting.axisY2.scale">inherit</option>
      <option name="charting.chart.bubbleMaximumSize">50</option>
      <option name="charting.chart.bubbleMinimumSize">10</option>
      <option name="charting.chart.bubbleSizeBy">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <chart>
      <searchName>Project Honey Pot - Threatscore AVG Last 4 Hours</searchName>
      <title>Threatscore AVG Last 4 Hours</title>
      <option name="charting.chart">fillerGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.rangeValues">[0,2,4,10,20,40]</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.gaugeColors">[0x84e900,0xffe800,0xf7912c,0xd85e3d,0xbf3030]</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <table>
      <searchName>Project Honey Pot - Unique Visitors Threatscoring_Last 24h</searchName>
      <title>Unique Visitors Threat scoring Last 24h</title>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">50</option>
      <option name="dataOverlayMode">heatmap</option>
      <option name="displayRowNumbers">true</option>
      <option name="drilldown">none</option>
    </table>
  </row>
  <row>
    <chart>
      <searchName>Project Honey Pot - Threatscore Timechart Last 4h</searchName>
      <title>Threatscore of Visitors Last 4h</title>
      <option name="charting.axisTitleY.text">Number of Page Impressions</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.primaryAxisTitle.text">Time</option>
      <option name="charting.secondaryAxis.scale">linear</option>
      <option name="charting.secondaryAxisTitle.text"></option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Project Honey Pot - Threatscore Timechart Last 24h</searchName>
      <title>Threatscore of Visitors Last 24h</title>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.primaryAxisTitle.text">Time</option>
      <option name="charting.secondaryAxisTitle.text">Threatscore Acitvity</option>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
      <option name="drilldown">none</option>
      <option name="maxLines">10</option>
      <option name="segmentation">full</option>
      <option name="softWrap">True</option>
    </chart>
  </row>
</form>