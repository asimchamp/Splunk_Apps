<form>
  <label>Station Analytics</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="time" token="selector" searchWhenChanged="true">
      <label>Select Time</label>
      <default>
        <earliest>-1d@d</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="station" searchWhenChanged="true">
      <label>Select Station</label>
      <search>
        <query>| pivot EnergyData EnergyLoad count(EnergyLoad) AS "count(EnergyLoad)" SPLITROW StationName AS "StationName" SPLITROW StationId AS StationId SORT 100 StationName ROWSUMMARY 0 COLSUMMARY 0 NUMCOLS 0 SHOWOTHER 1 |table StationName StationId</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <fieldForLabel>StationName</fieldForLabel>
      <fieldForValue>StationId</fieldForValue>
      <choice value="Select">Please Select</choice>
      <default>Select</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <chart>
        <title>Week Day Hourly Profile - Electricity</title>
        <search>
          <query>| pivot EnergyData EnergyLoad avg(ElectricityTotal) AS "Average of Electricity - Total" SPLITROW date_hour AS date_hour FILTER IsWeekend is WeekDay FILTER StationId is "$station$" SORT 100 date_hour ROWSUMMARY 0 COLSUMMARY 0 NUMCOLS 0 SHOWOTHER 1</query>
          <earliest>$selector.earliest$</earliest>
          <latest>$selector.latest$</latest>
        </search>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.legend.placement">none</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.chart">area</option>
        <option name="charting.axisTitleX.text">Hour</option>
        <option name="charting.axisTitleY.text">Avg Electricity Demand</option>
      </chart>
    </panel>
