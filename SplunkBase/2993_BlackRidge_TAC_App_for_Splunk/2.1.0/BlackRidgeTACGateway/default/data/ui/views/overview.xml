  <row>
    <panel>
      <title>Source Locality</title>
      <chart>
        <search>
          <query>index = * | eval locality=if(cidrmatch("192.168.0.0/16",src) or cidrmatch("10.0.0.0/8",src) or cidrmatch("172.16.0.0/12",src),"private","public") | stats count by locality</query>
