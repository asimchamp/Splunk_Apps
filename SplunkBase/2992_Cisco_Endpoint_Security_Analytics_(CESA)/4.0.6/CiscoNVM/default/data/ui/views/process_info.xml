<dashboard version="1.1" stylesheet="css/common.css">
  <search>
    <query>| rest /servicesNS/admin/CiscoNVM/configs/conf-nvm | fields fmc_ip | rename fmc_ip as managementIp</query>
    <earliest>-1d@d</earliest>
    <latest>@d</latest>
    <done>
      <eval token="fmcLink">$managementIp$</eval>
      <eval token="tokTime">$job.earliestTime$</eval>
      <eval token="tokEarliestTime">strftime(strptime($job.earliestTime$,"%Y/%m/%dT%H:%M:%S.%3N %p"),"%B %dth %y %I:%M %p")</eval>
      <eval token="tokLatestTime">strftime(strptime($job.latestTime$,"%Y/%m/%dT%H:%M:%S.%3N %p"),"%B %dth %y %I:%M %p")</eval>
    </done>
    <finalized>
      <condition match=" 'job.resultCount' != 0">
        <set token="fmcLink">$result.managementIp$</set>
      </condition>
      <condition>
        <set token="fmcLink">No FMC found</set>
      </condition>
    </finalized>
  </search>
  <label>Process info view for</label>
  <row>
    <panel id="drilldown">
      <title>Select the following source to view information for : $url:processName$ </title>
      <html>
          <div>    
              <a href="https://panacea.threatgrid.com/search/samples?qtype=checksum&amp;q=$url:processName$" target="_blank"><h2>ThreatGrid</h2></a>
              <a href="https://console.amp.cisco.com/search?query=$url:processName$" target="_blank"><h2>Amp</h2></a>
              <a href="https://visibility.amp.cisco.com/#/investigate?q=$url:processName$" target="_blank"><h2>Cisco Threat Response</h2></a>
              <a href="https://www.virustotal.com/gui/file/$url:processName$/detection" target="_blank"><h2>VirusTotal</h2></a>
          </div>
        </html>
    </panel>
  </row>
</dashboard>
