<view template="dashboard.html">    <label>Traffic Statistics</label>    <!-- Top Navigation -->    <module name="AccountBar" layoutPanel="appHeader"/>    <module name="AppBar" layoutPanel="navigationHeader"/>    <module name="TitleBar" layoutPanel="viewHeader" />        <module name="HiddenSearch" layoutPanel="panel_row2_col1" group="Top Protocols Used" autoRun="True">            <param name="search">sourcetype="pdml" packet | chart count(frame_protocols) by frame_protocols</param>            <module name="HiddenChartFormatter">                <param name="chart">pie</param>                <module name="FlashChart" />            </module>        </module>        <module name="HiddenSearch" layoutPanel="panel_row2_col2" group="Frame Sizes" autoRun="True">            <param name="search">sourcetype="pdml" packet | chart count(frame_len) by frame_len</param>            <module name="HiddenChartFormatter">                <param name="chart">pie</param>                <module name="FlashChart" />            </module>        </module>        <module name="HiddenSearch" layoutPanel="panel_row3_col1" group="Top UDP Source Ports" autoRun="True">            <param name="search">sourcetype="pdml" packet | chart count(udp_srcport) by udp_srcport</param>            <module name="HiddenChartFormatter">               <param name="chart">pie</param>               <module name="FlashChart" />            </module>        </module>    <module name="HiddenSearch" layoutPanel="panel_row3_col2" group="Top UDP Destination Ports" autoRun="True">        <param name="search">sourcetype="pdml" packet | chart count(udp_dstport) by udp_dstport</param>        <module name="HiddenChartFormatter">            <param name="chart">pie</param>            <module name="FlashChart" />        </module>    </module></view>