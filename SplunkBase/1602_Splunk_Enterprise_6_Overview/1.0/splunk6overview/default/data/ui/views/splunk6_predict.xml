<?xml version="1.0" encoding="UTF-8"?><form>
	<label>Predict - Flight Cancellations</label>
	<description/>
	<fieldset autoRun="true" submitButton="false">
		<input type="dropdown" token="s_origin">
			<label>Airport:</label>
			<choice value="*">ALL</choice>
			<populatingSearch fieldForLabel="sumOriginAirport" fieldForValue="sumOrigin">index=faa sourcetype="flightsummary"
| dedup sumOrigin
| sort +sumOriginAirport</populatingSearch>
			<default>*</default>
		</input>
	</fieldset>
	<row>
		<chart id="element1">
			<title>Prediction of Cancelled Flights (Using Entire 2012)</title>
			<searchString>index=faa sourcetype="flightsummary" sumOrigin="$s_origin$"
| timechart span=1d sum(sumFlightCount) as count_12_months
| predict count_12_months as predict_12_months future_timespan=60</searchString>
			<earliestTime>1325404800</earliestTime>
			<latestTime>1357027200</latestTime>
			<option name="charting.chart">line</option>
			<option name="height">400px</option>
			<option name="charting.legend.placement">bottom</option>
			<option name="charting.axisY.scale">linear</option>
			<option name="charting.axisX.scale">linear</option>
			<option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.rangeValues">["0","30","70","100"]</option>
			<option name="charting.chart.style">shiny</option>
			<option name="charting.gaugeColors">["84E900", "FFE800", "BF3030"]</option>
			<option name="charting.drilldown">1</option>
			<option name="charting.gaugeAutoRanges">1</option>
			<option name="height">400</option>
			<option name="charting.fieldColors">{ "count_12_months": 0x0000CD, "predict_12_months": 0x00BFFF}</option>
		</chart>
	</row>
	<row>
		<chart id="element2">
			<title>Prediction of Cancelled Flights (Using Q4 2012)</title>
			<searchString>index=faa sourcetype="flightsummary" sumOrigin="$s_origin$"
| timechart span=1d sum(sumFlightCount) as count_3_months
| predict count_3_months as predict_3_months future_timespan=60</searchString>
			<earliestTime>1349074800</earliestTime>
			<latestTime>1357027200</latestTime>
			<option name="charting.chart">line</option>
			<option name="height">400px</option>
			<option name="charting.legend.placement">bottom</option>
			<option name="charting.axisY.scale">linear</option>
			<option name="charting.axisX.scale">linear</option>
			<option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.rangeValues">["0","30","70","100"]</option>
			<option name="charting.chart.style">shiny</option>
			<option name="charting.gaugeColors">["84E900", "FFE800", "BF3030"]</option>
			<option name="charting.drilldown">1</option>
			<option name="charting.gaugeAutoRanges">1</option>
			<option name="height">400</option>
			<option name="charting.fieldColors">{ "count_3_months": 0x008000, "predict_3_months": 0x32CD32}</option>
		</chart>
	</row>
	<row>
		<chart id="element3">
			<title>Actual Flight Cancellations for Jan-Feb</title>
			<searchString>index=faa sourcetype="flightsummary" sumOrigin="$s_origin$"
| timechart span=1d sum(sumFlightCount) as count_actual</searchString>
			<earliestTime>1357027200</earliestTime>
			<latestTime>1362124800</latestTime>
			<option name="charting.chart">line</option>
			<option name="height">400px</option>
			<option name="charting.legend.placement">bottom</option>
			<option name="charting.axisY.scale">linear</option>
			<option name="charting.axisX.scale">linear</option>
			<option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.rangeValues">["0","30","70","100"]</option>
			<option name="charting.chart.style">shiny</option>
			<option name="charting.gaugeColors">["84E900", "FFE800", "BF3030"]</option>
			<option name="charting.drilldown">1</option>
			<option name="charting.gaugeAutoRanges">1</option>
			<option name="height">400</option>
			<option name="charting.fieldColors">{ "count_actual": FF0000}</option>
		</chart>
	</row>
	<row>
		<chart id="element4">
			<title>Prediction Overlay</title>
			<searchString>index=faa sourcetype="flightsummary" sumOrigin="$s_origin$" earliest="1325404800" latest="1357027200"
| timechart span=1d sum(sumFlightCount) as count_12_months
| predict count_12_months as predict_12_months future_timespan=60
| eval predict_12_months = round(predict_12_months,0)
| fields _time count_12_months predict_12_months
| sort -_time
| join _time type=outer [ search index=faa sourcetype="flightsummary" sumOrigin="$s_origin$" earliest="1349074800" latest="1357027200"
| timechart span=1d sum(sumFlightCount) as count_3_months
| predict count_3_months as predict_3_months future_timespan=60
| eval predict_3_months = round(predict_3_months,0)
| fields _time count_3_months predict_3_months ]
| sort -_time
| appendcols [ search index=faa sourcetype="flightsummary" sumOrigin="$s_origin$" earliest="1357027200" latest="1362124800"
| timechart span=1d sum(sumFlightCount) as count_actual
| sort -_time
| fields _time count_actual]
| fields - _timediff
| sort +_time</searchString>
			<earliestTime>1349074800</earliestTime>
			<latestTime>1357027200</latestTime>
			<option name="charting.chart">line</option>
			<option name="height">400px</option>
			<option name="charting.legend.placement">bottom</option>
			<option name="charting.axisY.scale">linear</option>
			<option name="charting.axisX.scale">linear</option>
			<option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.rangeValues">["0","30","70","100"]</option>
			<option name="charting.chart.style">shiny</option>
			<option name="charting.gaugeColors">["84E900", "FFE800", "BF3030"]</option>
			<option name="charting.drilldown">1</option>
			<option name="charting.gaugeAutoRanges">1</option>
			<option name="height">600</option>
			<option name="charting.fieldColors">{ "count_12_months": 0x0000CD, "predict_12_months": 0x00BFFF, "count_3_months": 0x008000, "predict_3_months": 0x32CD32, "count_actual": FF0000}</option>		
      </chart>
	</row>
</form>