<dashboard>
	<label>Cancelled Flights Analysis (simple)</label>
	<description>Analysis of Cancelled Flights for the month of January 2013</description>
	<row>
		<chart id="element4">
			<title>Cancellation Type over Time (Search Report)</title>
			<searchName>Cancellation Type Over Time</searchName>
			<option name="charting.axisX.scale">linear</option>
			<option name="charting.axisY.scale">linear</option>
			<option name="charting.chart">area</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.rangeValues">["0","30","70","100"]</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.chart.style">shiny</option>
			<option name="charting.drilldown">1</option>
			<option name="charting.gaugeAutoRanges">1</option>
			<option name="charting.gaugeColors">["84E900","FFE800","BF3030"]</option>
			<option name="height">300px</option>
			<option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
			<option name="charting.legend.placement">bottom</option>
		</chart>
		<table id="element3">
			<title>Cancellation Types (Inline Search)</title>
			<searchString>index=faa cancellation=* | top cancellation | fields - percent</searchString>
			<earliestTime>1357027200</earliestTime>
			<latestTime>1359705600</latestTime>
			<option name="charting.axisX.scale">linear</option>
			<option name="charting.axisY.scale">linear</option>
			<option name="charting.chart">bar</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.rangeValues">["0","30","70","100"]</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.chart.style">shiny</option>
			<option name="charting.drilldown">1</option>
			<option name="charting.gaugeAutoRanges">1</option>
			<option name="charting.gaugeColors">["84E900","FFE800","BF3030"]</option>
			<option name="height">300px</option>
			<option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
			<option name="charting.legend.placement">none</option>
			<option name="wrap">true</option>
			<option name="displayRowNumbers">true</option>
			<option name="drilldown">row</option>
		</table>
	</row>
	<row>
		<table id="element1">
			<title>Top Airports of Cancelled Flights (Inline Search)</title>
			<searchString>index=faa cancellation=* 
| chart count as Total 
count(eval(cancellation=="Weather")) as "Weather" 
count(eval(cancellation=="National Air System")) as "National Air System" 
count(eval(cancellation=="Security")) as "Security" 
count(eval(cancellation=="Carrier")) as "Carrier" 
by OriginAirport
| sort - Total</searchString>
			<earliestTime>1357027200</earliestTime>
			<latestTime>1359705600</latestTime>
			<option name="wrap">true</option>
			<option name="displayRowNumbers">true</option>
			<option name="drilldown">row</option>
		</table>
	</row>
</dashboard>
