<view template="dashboard.html">
	<label>Deny Activity Summary</label>
	<module name="AccountBar" layoutPanel="navigationHeader"/>
	<module name="AppBar" layoutPanel="navigationHeader"/>
	<module name="Message" layoutPanel="messaging">
		<param name="filter">*</param>
		<param name="clearOnJobDispatch">False</param>
		<param name="maxSize">1</param>
	</module>
	<module name="TimeRangePicker" layoutPanel="panel_row1_col1" group="Top 10 Denied Source IP's">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperFW Top 10 Denied Source</param>
      <module name="JobProgressIndicator"></module>
      <module name="Paginator">
        <param name="count">15</param>
        <param name="maxPages">15</param>
        <param name="entityName">results</param>
        <module name="SimpleResultsTable">
          <param name="drilldown">row</param>
          <param name="entityName">results</param>
          <module name="ConvertToDrilldownSearch">
            <module name="ViewRedirector">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row1_col2" group="Top 10 Denied Destination IP's">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperFW Top 10 Denied Destination</param>
      <module name="JobProgressIndicator"></module>
      <module name="Paginator">
        <param name="count">15</param>
        <param name="maxPages">15</param>
        <param name="entityName">results</param>
        <module name="SimpleResultsTable">
          <param name="drilldown">row</param>
          <param name="entityName">results</param>
          <module name="ConvertToDrilldownSearch">
            <module name="ViewRedirector">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row1_col3" group="Top 10 Denied Destination Ports">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperFW Top 10 Denied Des Port</param>
      <module name="JobProgressIndicator"></module>
      <module name="Paginator">
        <param name="count">15</param>
        <param name="maxPages">15</param>
        <param name="entityName">results</param>
        <module name="SimpleResultsTable">
          <param name="drilldown">row</param>
          <param name="entityName">results</param>
          <module name="ConvertToDrilldownSearch">
            <module name="ViewRedirector">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row2_col1" group="Firewall Denied Activity Report">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperFW Denied Log Report</param>
      <module name="JobProgressIndicator"></module>
      <module name="Paginator">
        <param name="count">20</param>
        <param name="maxPages">15</param>
        <param name="entityName">results</param>
        <module name="SimpleResultsTable">
          <param name="drilldown">row</param>
          <param name="entityName">results</param>
          <module name="ConvertToDrilldownSearch">
            <module name="ViewRedirector">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
</view>