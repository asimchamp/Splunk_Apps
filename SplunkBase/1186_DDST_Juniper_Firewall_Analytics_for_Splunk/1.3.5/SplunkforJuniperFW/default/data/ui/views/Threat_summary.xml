<view template="dashboard.html">
  <label>Threat Summary</label>
  <module name="AccountBar" layoutPanel="navigationHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="navigationHeader">
    <param name="showActionsMenu">False</param>
  </module>
  <module name="ViewstateAdapter" layoutPanel="panel_row1_col1" group="Real Time Hourly Firewall Statistics">
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="savedSearch">JuniperFW Get Device Count</param>
      <module name="ViewstateAdapter">
        <param name="suppressionList"/>
        <module name="HiddenFieldPicker">
          <param name="strictMode">True</param>
          <module name="SingleValue">
            <param name="beforeLabel">No Devices:</param>
          </module>
        </module>
      </module>
    </module>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="savedSearch">JuniperFW Get Deny Count</param>
      <module name="ViewstateAdapter">
        <param name="suppressionList"/>
        <item>charting.seriesColors</item>
        <item>classField</item>
        <module name="SingleValue">
          <param name="classField">'severe'</param>
          <param name="beforeLabel">Blocked Traffic:</param>
        </module>
      </module>
    </module>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="savedSearch">JuniperFW Get Permit Count</param>
      <module name="ViewstateAdapter">
        <param name="suppressionList"/>
        <module name="HiddenFieldPicker">
          <param name="strictMode">True</param>
          <module name="SingleValue">
            <param name="beforeLabel">Allowed Traffic:</param>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="ViewstateAdapter" layoutPanel="panel_row1_col2" group="Real Time Hourly Threat Statistics">
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="savedSearch">JuniperDI Signatures Per Hour</param>
	  <module name="ViewstateAdapter">
				<param name="suppressionList">
					<item>charting.seriesColors</item>
					<item>classField</item>
				</param>
				<module name="HiddenFieldPicker">
					<param name="strictMode">True</param>
					<module name="JobProgressIndicator">
						<module name="SingleValue">
							<param name="classField">range</param>
							<param name="beforeLabel">Deep Inspection Events:</param>
						</module>
					</module>
				</module>
			</module>
    </module>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="savedSearch">Screen Event Per Hour</param>
	  <module name="ViewstateAdapter">
				<param name="suppressionList">
					<item>charting.seriesColors</item>
					<item>classField</item>
				</param>
				<module name="HiddenFieldPicker">
					<param name="strictMode">True</param>
					<module name="JobProgressIndicator">
						<module name="SingleValue">
							<param name="classField">range</param>
							<param name="beforeLabel">Screen Events:</param>
						</module>
					</module>
				</module>
			</module>
    </module>
	<module name="HiddenSavedSearch" autoRun="True">
      <param name="savedSearch">PHPSC Event Per Hour</param>
	  <module name="ViewstateAdapter">
				<param name="suppressionList">
					<item>charting.seriesColors</item>
					<item>classField</item>
				</param>
				<module name="HiddenFieldPicker">
					<param name="strictMode">True</param>
					<module name="JobProgressIndicator">
						<module name="SingleValue">
							<param name="classField">range</param>
							<param name="beforeLabel">High Risk Events:</param>
						</module>
					</module>
				</module>
			</module>
    </module>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row2_col1" group="Firewall Activity Summary">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperFW Device Summary</param>
      <module name="JobProgressIndicator"></module>
      <module name="Paginator">
        <param name="count">15</param>
        <param name="maxPages">15</param>
        <param name="entityName">results</param>
        <module name="SimpleResultsTable">
          <param name="drilldown">row</param>
          <param name="entityName">results</param>
          <module name="ConvertToDrilldownSearch">
            <module name="ViewRedirector">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row2_col2" group="Top 10 Malicious Destinations">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 60 minutes</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">Top 10 PHPSC Risk</param>
      <module name="JobProgressIndicator"></module>
      <module name="SimpleResultsTable">
        <param name="drilldown">row</param>
        <param name="entityName">results</param>
        <module name="ConvertToDrilldownSearch">
          <module name="ViewRedirector">
            <param name="viewTarget">flashtimeline</param>
          </module>
        </module>
      </module>
    </module>
   <module name="StaticContentSample">
      <param name="text">Malicious destinations risk score is provided by Defensive ISS and other sources.  To enable this feature register at <![CDATA[<a href="http://www.projecthoneypot.org/index.php"> Project Honey Pot</a>]]> to obtain your API key. Please follow the instruction <![CDATA[<a href="/app/SplunkforJuniperFW/support">provided here</a>]]>.</param>
  </module>
 </module>
  <module name="TimeRangePicker" layoutPanel="panel_row3_col2" group="Deep Inspection Activity Summary">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 24 hours</param>
  <module name="StaticContentSample">
      <param name="text">Deep Inspection Events Status by Device</param>
  </module>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperDI Event Status by Device</param>
      <module name="JobProgressIndicator"></module>
      <module name="Paginator">
        <param name="count">15</param>
        <param name="maxPages">15</param>
        <param name="entityName">results</param>
        <module name="SimpleResultsTable">
          <param name="drilldown">row</param>
          <param name="entityName">results</param>
          <module name="ConvertToDrilldownSearch">
            <module name="ViewRedirector">
              <param name="viewTarget">flashtimeline</param>
            </module>
          </module>
        </module>
      </module>
    </module>
      <module name="StaticContentSample">
      <param name="text">Deep Inspection Top 10 Prevalant Signatures</param>
  </module>
	<module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperDI Top 10 Prevalant DI Signatures</param>
      <module name="JobProgressIndicator"></module>
      <module name="SimpleResultsTable">
        <param name="drilldown">row</param>
        <param name="entityName">results</param>
        <module name="ConvertToDrilldownSearch">
          <module name="ViewRedirector">
            <param name="viewTarget">flashtimeline</param>
          </module>
        </module>
      </module>
    </module>
	      <module name="StaticContentSample">
      <param name="text">Deep Inspection Top 10 Countries</param>
  </module>
	<module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperDI Top 10 Countries</param>
      <module name="JobProgressIndicator"></module>
      <module name="SimpleResultsTable">
        <param name="drilldown">row</param>
        <param name="entityName">results</param>
        <module name="ConvertToDrilldownSearch">
          <module name="ViewRedirector">
            <param name="viewTarget">flashtimeline</param>
          </module>
        </module>
      </module>
    </module>
  </module>
  <module name="TimeRangePicker" layoutPanel="panel_row3_col1" group="Top 10 External Destination">
    <param name="searchWhenChanged">True</param>
    <param name="selected">Last 60 minutes</param>
    <module name="HiddenSavedSearch" autoRun="True">
      <param name="useHistory">false</param>
      <param name="savedSearch">JuniperFW Top 10 Countries</param>
      <module name="JobProgressIndicator"></module>
      <module name="SimpleResultsTable">
        <param name="drilldown">row</param>
        <param name="entityName">results</param>
        <module name="ConvertToDrilldownSearch">
          <module name="ViewRedirector">
            <param name="viewTarget">flashtimeline</param>
          </module>
        </module>
      </module>
    </module>
  </module>
</view>
