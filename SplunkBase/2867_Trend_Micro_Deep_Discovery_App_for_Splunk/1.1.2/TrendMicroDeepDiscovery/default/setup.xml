<setup>

  <block title="Deep Discovery Event Type" endpoint="configs/conf-eventtypes/" entity="user_event">
    <text>
      &lt;i&gt; Specify the index name and event type of your Deep Discovery events.&lt;/i&gt;
    </text>
    <input field="search">
      <label>Index and source type:</label>
      <type>string</type>
    </input>
  </block>  

  <block title="Web Access Log Event Type" endpoint="configs/conf-eventtypes/" entity="web_event">
    <text>
      &lt;i&gt; Specify the index name and event type of your Web Access Log events.&lt;/i&gt;
    </text>
    <input field="search">
      <label>Index and source type:</label>
      <type>string</type>
    </input>
  </block>  

  <block title="Proxy Settings" endpoint="configs/conf-proxy/" entity="proxy">
    <input field="enable">
      <label>Use a HTTP proxy server for license</label>
      <type>bool</type>
    </input>
    <input field="host">
      <label>Host name or IP address:</label>
      <type>string</type>
    </input>
    <input field="port">
      <label>Port:</label>
      <type>string</type>
    </input>
    <text>
      &lt;i&gt; Proxy server authentication:&lt;/i&gt;
    </text>
    <input field="username">
      <label>User name:</label>
      <type>string</type>
    </input>
    <input field="password">
      <label>Password:</label>
      <type>password</type>
    </input>
  </block>  

</setup>
