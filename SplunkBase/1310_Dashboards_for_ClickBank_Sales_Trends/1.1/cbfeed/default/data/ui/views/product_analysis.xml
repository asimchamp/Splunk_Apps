<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Product Analysis</label>
  <row>
    <chart>
      <searchName>Product Analysis</searchName>
      <title>Top 10 Products by Gravity</title>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <table>
      <searchName>Top 10 Products Ranked by Highest Avg Earnings Per Sale</searchName>
      <title>Top 10 Products by Highest Avg Earnings Per Sale</title>
      <option name="count">10</option>
      <option name="dataOverlayMode">heatmap</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchName>Top 10 Products Ranked by Highest Renewals</searchName>
      <title>Top 10 Products by Highest Renewal</title>
      <option name="count">10</option>
      <option name="dataOverlayMode">none</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <chart>
      <searchName>Top 10 Gravity Gainers</searchName>
      <title>Top 10 Gravity Gainers</title>
      <option name="charting.chart">column</option>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Top 10 Gravity Losers</searchName>
      <title>Top 10 Gravity Losers</title>
      <option name="charting.chart">bar</option>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
</dashboard>
