<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Category Analysis</label>
  <row>
    <chart>
      <searchName>Top 10 Categories with Highest Avg Gravity - At Least 10 Products - Pie</searchName>
      <title>Top 10 Highest Avg Gravity Categories (&gt; 10 Products)</title>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.showMarkers">false</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <chart>
      <searchName>Top 10 Categories with Highest median Gravity - At Least 10 Products - Pie</searchName>
      <title>Top 10 Highest Median Gravity Categories (&gt;10 Products)</title>
      <option name="charting.chart">pie</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Top 10 Categories by Product Count</searchName>
      <title>Top 10 Categories by Product Count</title>
      <option name="charting.chart">column</option>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Top 10 Categories with Highest Average Earnings Per Sale</searchName>
      <title>Top 10 Categories with Highest Avg Earnings Per Sale</title>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Top 10 Categories with Highest Average Rebill Rate</searchName>
      <title>Top 10 Categories With Highest Avg Rebill Rate</title>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
</dashboard>
