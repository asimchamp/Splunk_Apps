<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Marketplace Overview</label>
  <row>
    <single>
      <searchName>Number of Days in Index</searchName>
      <title>Number of Days Indexed</title>
    </single>
    <single>
      <searchName>Number of Products Indexed</searchName>
      <title>Unique Products Indexed</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
    <single>
      <searchName>Products with Repeat Billing</searchName>
      <title>Products with Repeat Billing</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
  </row>
  <row>
    <chart>
      <searchName>Gravity by Product Id - Multi-Series</searchName>
      <title>Product Gravity Distribution</title>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.showMarkers">false</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  <row>
    <single>
      <searchName>Avg Gravity for All Products</searchName>
      <title>Avg Gravity - All Products</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
    <single>
      <searchName>Median Gravity for All Products</searchName>
      <title>Median Gravity - All Products</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
    <single>
      <searchName>Number of Products with Gravity &gt;100</searchName>
      <title>Products With Gravity &gt; 100</title>
      <option name="count">50</option>
      <option name="displayRowNumbers">true</option>
    </single>
  </row>
  <row>
    <table>
      <searchName>Top 20 - Most Recent Products</searchName>
      <title>Top 20 Most Recent Products</title>
    </table>
  </row>
  <row>
    <chart>
      <searchName>Number of Distinct Products By Day</searchName>
      <title>Number of Distinct Products Indexed By Day</title>
      <option name="charting.chart">area</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
</dashboard>
