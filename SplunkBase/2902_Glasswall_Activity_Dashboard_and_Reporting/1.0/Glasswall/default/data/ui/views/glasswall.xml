        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <title>PowerPoint Document Components Sanitised</title>
      <chart>
        <search>
          <query>sourcetype=GWExportCSV Outcome=Sanitised 
| eval PF=if(Count_Of_PowerPoint_Embedded_File_Sanitisations &gt;0,1,0) 
| eval PH=if(Count_Of_PowerPoint_Hyperlink_Sanitisations &gt;0,1,0) 
| eval PM=if(Count_Of_PowerPoint_Macro_Sanitisations &gt;0,1,0) 
| timechart span=1y sum(PM) as PowerPoint_Macros sum(PH) as PowerPoint_Hyperlinks sum(PF) as PowerPoint_Embedded_files</query>
