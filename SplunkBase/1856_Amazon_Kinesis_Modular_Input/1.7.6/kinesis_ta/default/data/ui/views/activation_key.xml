<dashboard version="1.1" script="activation_key.js" stylesheet="activation_key.css" hideEdit="true">

 <label>Activation Key</label>
    <row>
        <panel>
            <html>

              <h1>Enter your Activation Key</h1>
              <p>
                  If you don't yet have a key , please visit <a href="http://www.baboonbones.com/#activation" target="_blank">http://www.baboonbones.com/#activation</a> to obtain a trial or non-expiring key.
              </p>
               <hr/>
                
              <form>
                <div class="activation_key_div_input_fields">

                  <label class="activation_key_form_label" for="activation_key" >Activation Key</label>
                  <input type="password" id="activation_key"  name="activation_key" /><br/>

                  </div>

                   <div id="activation_key_form_buttons">

                      <button type="button" id="activation_key_cancel_button" class="activation_key_cancel_button"> Cancel </button> 
                      <button type="button" id="activation_key_submit_button" class="activation_key_submit_button"> Save </button>

                   </div>

                </form>


            </html>
        </panel>
    </row>
</dashboard>