{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Account Name",
                                "field": "name"
                            },
                            {
                                "label": "Client ID",
                                "field": "client_id"
                            }
                        ]
                    },
                    "name": "account",
                    "entity": [
                        {
                            "type": "text",
                            "label": "Account Name",
                            "required": true,
                            "help": "Enter a unique name for this box account.",
                            "options": {
                                "placeholder": "Required"
                            },
                            "validators": [
                                {
                                    "type": "string",
                                    "maxLength": 50,
                                    "errorMsg": "Account Name must not exceed 50 characters.",
                                    "minLength": 1
                                },
                                {
                                    "type": "regex",
                                    "errorMsg": "Account Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                }
                            ],
                            "field": "name"
                        },
                        {
                            "type": "text",
                            "label": "Endpoint URL",
                            "help": "Enter the endpoint URL.",
                            "field": "endpoint",
                            "defaultValue": "account.box.com",
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "type": "oauth",
                            "label": "Authentication",
                            "field": "oauth",
                            "help": "",
                            "options": {
                                "auth_type": [
                                    "oauth"
                                ],
                                "oauth": [
                                    {
                                        "oauth_field": "client_id",
                                        "label": "Client ID",
                                        "field": "client_id",
                                        "encrypted": false,
                                        "help": "Enter Box Client ID.",
                                        "options": {
                                            "placeholder": "Required"
                                        }
                                    },
                                    {
                                        "oauth_field": "client_secret",
                                        "label": "Client Secret",
                                        "field": "client_secret",
                                        "encrypted": true,
                                        "help": "Enter Box Client Secret.",
                                        "options": {
                                            "placeholder": "Required"
                                        }
                                    },
                                    {
                                        "oauth_field": "redirect_url",
                                        "label": "Redirect URL",
                                        "field": "redirect_url",
                                        "encrypted": false,
                                        "help": "Copy and paste this URL into \"OAuth 2.0 Redirect URI\" field on Box Application configuration page."
                                    }
                                ],
                                "auth_code_endpoint": "/api/oauth2/authorize",
                                "access_token_endpoint": "/api/oauth2/token",
                                "oauth_timeout": 30,
                                "oauth_state_enabled": true
                            }
                        }
                    ],
                    "title": "Box Account",
                    "hook": {
                        "src": "account_hook.2.0.0",
                        "type": "external"
                    }
                },
                {
                    "title": "Proxy",
                    "name": "proxy",
                    "entity": [
                        {
                            "type": "checkbox",
                            "field": "proxy_enabled",
                            "label": "Enable Proxy"
                        },
                        {
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "maxLength": 4096,
                                    "minLength": 1,
                                    "errorMsg": "Maximum length allowed for host is 4096"
                                },
                                {
                                    "pattern": "^[a-zA-Z0-9:][a-zA-Z0-9\\.\\-:]+$",
                                    "errorMsg": "Proxy Host is invalid",
                                    "type": "regex"
                                }
                            ],
                            "field": "proxy_url",
                            "label": "Proxy Host",
                            "required": true,
                            "options": {
                                "placeholder": "Required"
                            }
                        },
                        {
                            "type": "text",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        65535
                                    ]
                                }
                            ],
                            "field": "proxy_port",
                            "label": "Proxy Port",
                            "required": true,
                            "options": {
                                "placeholder": "Required"
                            }
                        },
                        {
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "maxLength": 50,
                                    "minLength": 0,
                                    "errorMsg": "Maximum length allowed for username is 50"
                                }
                            ],
                            "field": "proxy_username",
                            "label": "Proxy Username"
                        },
                        {
                            "type": "text",
                            "encrypted": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "maxLength": 8192,
                                    "minLength": 0,
                                    "errorMsg": "Maximum length allowed for password is 8192"
                                }
                            ],
                            "field": "proxy_password",
                            "label": "Proxy Password"
                        },
                        {
                            "type": "checkbox",
                            "field": "proxy_rdns",
                            "label": "Use Proxy for DNS Resolution"
                        },
                        {
                            "type": "singleSelect",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "http",
                                        "label": "http"
                                    },
                                    {
                                        "value": "socks5",
                                        "label": "socks5"
                                    }
                                ]
                            },
                            "defaultValue": "http",
                            "field": "proxy_type",
                            "label": "Proxy Type",
                            "required": true
                        }
                    ]
                },
                {
                    "entity": [
                        {
                            "type": "singleSelect",
                            "label": "Logging level",
                            "help": "(DEBUG, INFO, WARN, ERROR or CRITICAL)",
                            "defaultValue": "INFO",
                            "field": "loglevel",
                            "required": true,
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG",
                                        "label": "DEBUG"
                                    },
                                    {
                                        "value": "INFO",
                                        "label": "INFO"
                                    },
                                    {
                                        "value": "WARN",
                                        "label": "WARNING"
                                    },
                                    {
                                        "value": "ERROR",
                                        "label": "ERROR"
                                    },
                                    {
                                        "value": "CRITICAL",
                                        "label": "CRITICAL"
                                    }
                                ],
                                "disableSearch": true
                            }
                        }
                    ],
                    "name": "logging",
                    "title": "Logging"
                }
            ],
            "description": "Set up your add-on",
            "title": "Configuration"
        },
        "inputs": {
            "table": {
                "actions": [
                    "edit",
                    "enable",
                    "delete",
                    "clone"
                ],
                "header": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Account",
                        "field": "account",
                        "customCell": {
                            "src": "custom_input_cell",
                            "type": "external"
                        }
                    },
                    {
                        "label": "Endpoint",
                        "field": "rest_endpoint",
                        "customCell": {
                            "src": "custom_input_cell",
                            "type": "external"
                        }
                    },
                    {
                        "label": "Interval",
                        "field": "duration",
                        "customCell": {
                            "src": "custom_input_cell",
                            "type": "external"
                        }
                    },
                    {
                        "field": "disabled",
                        "label": "Status",
                        "mapping": {
                            "false": "Enabled",
                            "true": "Disabled"
                        }
                    }
                ],
                "moreInfo": [
                    {
                        "label": "Input name",
                        "field": "name"
                    },
                    {
                        "label": "Account",
                        "field": "account"
                    },
                    {
                        "label": "Endpoint",
                        "field": "rest_endpoint"
                    },
                    {
                        "label": "Interval",
                        "field": "duration"
                    },
                    {
                        "label": "Interval",
                        "field": "interval"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    }
                ]
            },
            "description": "",
            "services": [
                {
                    "hook": {
                        "src": "input_hook.2.0.0",
                        "type": "external"
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "required": true,
                            "help": "Enter a unique name for the input",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "maxLength": 100,
                                    "errorMsg": "Maximum length allowed for name is 100",
                                    "minLength": 1
                                }
                            ],
                            "field": "name",
                            "options": {
                                "disableonEdit": true,
                                "placeholder": "Required"
                            }
                        },
                        {
                            "type": "text",
                            "label": "Input Name",
                            "required": false,
                            "help": "Enter a unique name for the input",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "maxLength": 100,
                                    "errorMsg": "Maximum length allowed for name is 100",
                                    "minLength": 1
                                }
                            ],
                            "field": "input_name",
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "type": "singleSelect",
                            "label": "Box Account",
                            "options": {
                                "referenceName": "account"
                            },
                            "field": "account",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "label": "Endpoint",
                            "field": "rest_endpoint",
                            "help": "Select endpoint",
                            "required": true,
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "value": "events",
                                        "label": "Events (admin_logs)"
                                    },
                                    {
                                        "value": "users",
                                        "label": "Users"
                                    },
                                    {
                                        "value": "folders",
                                        "label": "Folders"
                                    },
                                    {
                                        "value": "groups",
                                        "label": "Groups"
                                    }
                                ],
                                "disableSearch": true
                            }
                        },
                        {
                            "type": "checkbox",
                            "field": "collect_folder",
                            "label": "Collect folder metadata",
                            "defaultValue": 1,
                            "required": false
                        },
                        {
                            "type": "checkbox",
                            "field": "collect_collaboration",
                            "label": "Collect folder collaboration",
                            "defaultValue": 1,
                            "required": false
                        },
                        {
                            "type": "checkbox",
                            "field": "collect_file",
                            "label": "Collect file metadata",
                            "defaultValue": 1,
                            "required": false
                        },
                        {
                            "type": "checkbox",
                            "field": "collect_task",
                            "label": "Collect tasks and comments",
                            "defaultValue": 1,
                            "required": false
                        },
                        {
                            "type": "radio",
                            "label": "Use existing data input?",
                            "field": "reuse_checkpoint",
                            "defaultValue": "yes",
                            "help": "A data input with this name already exists. Would you like to continue collecting data using this input?",
                            "required": false,
                            "options": {
                                "items": [
                                    {
                                        "value": "yes",
                                        "label": "Yes"
                                    },
                                    {
                                        "value": "no",
                                        "label": "No"
                                    }
                                ],
                                "display": false
                            }
                        },
                        {
                            "type": "text",
                            "field": "created_after",
                            "required": false,
                            "label": "Collect since timestamp",
                            "help": "The date and time, after converting to UTC in \"YYYY-MM-DDThh:mm:ss\" format e.g. 2020-01-30T14:00:30, after which to collect data. \nThe default is last 90 days.",
                            "tooltip": "Changing this parameter may result in gaps or duplication in data collection.",
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^([0-9]{4})-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T([01][0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])$",
                                    "errorMsg": "Enter valid date format \"YYYY-MM-DDThh:mm:ss\""
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "label": "Interval",
                            "required": true,
                            "help": "Time interval of the data input, in seconds.",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        31536000
                                    ]
                                },
                                {
                                    "type": "regex",
                                    "errorMsg": "Field Interval must be an integer",
                                    "pattern": "^\\d+$"
                                }
                            ],
                            "field": "duration"
                        },
                        {
                            "type": "text",
                            "label": "Delay",
                            "field": "event_delay",
                            "required": false,
                            "defaultValue": 0,
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Delay must be an integer.",
                                    "pattern": "^[1-9]\\d*$|^\\d*$"
                                }
                            ]
                        },
                        {
                            "type": "singleSelect",
                            "label": "Index",
                            "defaultValue": "default",
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "maxLength": 80,
                                    "errorMsg": "Maximum length allowed for index is 80",
                                    "minLength": 1
                                }
                            ],
                            "field": "index",
                            "options": {
                                "createSearchChoice": true,
                                "endpointUrl": "data/indexes",
                                "denyList": "^_.*$"
                            }
                        },
                        {
                            "type": "text",
                            "field": "folder_fields",
                            "label": "Folder Fields",
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "type": "text",
                            "field": "file_fields",
                            "label": "File Fields",
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "type": "text",
                            "field": "task_fields",
                            "label": "Task Fields",
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "type": "text",
                            "field": "comment_fields",
                            "label": "Comment Fields",
                            "options": {
                                "display": false
                            }
                        },
                        {
                            "type": "text",
                            "field": "user_fields",
                            "label": "User Fields",
                            "options": {
                                "display": false
                            }
                        }
                    ],
                    "name": "box_service",
                    "title": "Historical Querying Inputs"
                },
                {
                    "hook": {
                        "src": "input_hook_lmservice",
                        "type": "external"
                    },
                    "entity": [
                        {
                            "field": "name",
                            "type": "text",
                            "label": "Name",
                            "required": true,
                            "help": "Enter a unique name for the input",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "maxLength": 100,
                                    "errorMsg": "Maximum length allowed for name is 100",
                                    "minLength": 1
                                }
                            ],
                            "options": {
                                "placeholder": "Required"
                            }
                        },
                        {
                            "type": "singleSelect",
                            "label": "Box Account",
                            "options": {
                                "referenceName": "account"
                            },
                            "field": "account",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "label": "Endpoint",
                            "field": "rest_endpoint",
                            "defaultValue": "events",
                            "help": "Live Monitoring Input only support Events endpoint using admin_logs_streaming API. To use other endpoints, please select Historical Querying Inputs option from Create New Input button on Inputs page.",
                            "required": true,
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "value": "events",
                                        "label": "Events (admin_logs_streaming)"
                                    }
                                ],
                                "disableSearch": true
                            }
                        },
                        {
                            "type": "radio",
                            "label": "Use existing data input?",
                            "field": "reuse_checkpoint",
                            "defaultValue": "yes",
                            "help": "A data input with this name already exists. Would you like to continue collecting data using this input?",
                            "required": false,
                            "options": {
                                "items": [
                                    {
                                        "value": "yes",
                                        "label": "Yes"
                                    },
                                    {
                                        "value": "no",
                                        "label": "No"
                                    }
                                ],
                                "display": false
                            }
                        },
                        {
                            "type": "text",
                            "label": "Interval",
                            "required": true,
                            "defaultValue": 120,
                            "help": "Time interval of the data input, in seconds.",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        31536000
                                    ]
                                },
                                {
                                    "type": "regex",
                                    "errorMsg": "Field Interval must be an integer",
                                    "pattern": "^\\d+$"
                                }
                            ],
                            "field": "interval"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Index",
                            "defaultValue": "default",
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "maxLength": 80,
                                    "errorMsg": "Maximum length allowed for index is 80",
                                    "minLength": 1
                                }
                            ],
                            "field": "index",
                            "options": {
                                "createSearchChoice": true,
                                "endpointUrl": "data/indexes",
                                "denyList": "^_.*$"
                            }
                        }
                    ],
                    "name": "box_live_monitoring_service",
                    "title": "Live Monitoring Inputs"
                }
            ],
            "title": "Inputs"
        }
    },
    "meta": {
        "version": "3.9.0",
        "displayName": "Splunk_TA_box",
        "restRoot": "Splunk_TA_box",
        "name": "Splunk_TA_box",
        "schemaVersion": "0.0.3"
    }
}