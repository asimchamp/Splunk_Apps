<!--SplunkforCheckpoint - submitted by Simon O'Brien - sobrien@splunk.com--><form stylesheet="single_trend.css,font_style.css">
  <label>Firewall</label>

  <fieldset submitButton="false" autoRun="true">
    <input type="time" token="time_display" searchWhenChanged="true">
      <label>Time to display:</label>
      <default>
        <earliest>-60m@m</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="index_tok" searchWhenChanged="true">
      <label>Select an index:</label>
      <prefix>index="</prefix>
      <suffix>"</suffix>
      <!-- Define the default value -->
      <default>main</default>
      <!-- Hard-code the choice for "All" -->
      <!-- Define the other choices with a populating search -->
      <search>
        <query>| eventcount summarize=false index=* | dedup index | fields index</query>
      </search>
      <fieldForLabel>index</fieldForLabel>
      <fieldForValue>index</fieldForValue>
    </input>
    <input type="dropdown" token="sourcetype_tok" searchWhenChanged="true">
      <label>Select a sourcetype:</label>
      <prefix>sourcetype="</prefix>
      <suffix>"</suffix>
      <!-- Define the default value -->
      <default>opsec</default>
      <!-- Hard-code the choice for "All" -->
      <!-- Define the other choices with a populating search -->
      <search>
        <query>|metadata type=sourcetypes index=*  | table sourcetype</query>
      </search>
      <fieldForLabel>sourcetype</fieldForLabel>
      <fieldForValue>sourcetype</fieldForValue>
    </input>
    <input type="dropdown" token="orig" searchWhenChanged="true">
      <label>Firewall Address</label>
      <prefix>orig="</prefix>
      <suffix>"</suffix>
      <choice value="*">All</choice>
      <search>
        <query>$index_tok$ $sourcetype_tok$ | dedup orig | table orig</query>
      </search>
      <fieldForLabel>orig</fieldForLabel>
      <fieldForValue>orig</fieldForValue>
      <default>*</default>
    </input>
  </fieldset>
  <row>
      <panel>
    <title>Trend of top 15 Accepted Services</title>
    <chart>
      <search>
        <query>$index_tok$ $sourcetype_tok$  product="VPN-1 &amp; FireWall-1" $orig$ | timechart count by service_id limit=15 usenull=f useother=f</query>
        <earliest>$time_display.earliest$</earliest>
        <latest>$time_display.latest$</latest>
      </search>
      <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
      <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisTitleY2.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
