<form>
  <label>Process Directory Autostarts</label>
  <description></description>
  <fieldset autoRun="true" submitButton="false">
    <input type="time" searchWhenChanged="true">
      <default>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <chart>
        <title>New Enterprise Process AutoStarts</title>
        <search>
          <query>sourcetype=ziften* message_type=autostarts|stats min(_time) as _time by imagefilemd5|timechart count</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.chart.nullValueMode">zero</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.placement">right</option>
        <option name="count">10</option>
        <option name="displayRowNumbers">true</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">log</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      </chart>
    </panel>
    <panel>
      <table>
        <title>Most Popular Process AutoStarts</title>
        <search>
          <query>sourcetype=ziften* 
message_type=autostarts|eval basename = replace(imagefilepath, ".*\\\\", "")|stats dc(agentguid) as prev by basename|sort -prev</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>New Enterprise Process Autostarts</title>
        <search>
          <query>sourcetype=ziften* message_type=autostarts|stats dc(agentguid) as autostart_prev min(_time) as "First Seen" first(entrypath) as sample_entrypoint first(imagefilepath) as "Sample Path" by imagefilemd5|convert ctime("First Seen")|lookup image_info imagefilemd5|sort -"First Seen"|fields "First Seen" companyname fileversion processversion autostart_prev sample_entrypoint "Sample Path"</query>
          <earliest>0</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Systems With The Most Process AutoStarts</title>
        <search>
          <query>sourcetype=ziften* earliest=-14d message_type=autostarts|eval basename = replace(imagefilepath, ".*\\\\", "")|lookup system_info agentguid|eval computername=if(isnull(computername), agentguid, computername)|stats dc(imagefilepath) as "Total AutoStarts" values(computername) as Systems values(basename) as AutoStarts by agentguid|sort -"Total AutoStarts" |fields Systems "Total AutoStarts" AutoStarts</query>
          <earliest>0</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
</form>