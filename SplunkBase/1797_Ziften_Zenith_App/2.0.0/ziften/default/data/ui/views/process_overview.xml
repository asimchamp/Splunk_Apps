<form>
  <label>Process Overview</label>
  <fieldset autoRun="true" submitButton="false">
    <input type="time" searchWhenChanged="true">
      <default>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <chart>
        <title>System Process Counts Over Time</title>
        <search>
          <query>sourcetype=ziften* message_type=processinventory|streamstats dc(imagefilemd5) as totalprocs by agentguid|timechart max(totalprocs) as "Max Processes" avg(totalprocs) as "Avg Processes" min(totalprocs) as "Min Processes"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.chart.nullValueMode">zero</option>
        <option name="count">10</option>
        <option name="displayRowNumbers">true</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>New Enterprise Binaries</title>
        <search>
          <query>|inputlookup image.csv|rename first_seen as _time|timechart count by OS|sort _time</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="charting.chart">column</option>
        <option name="count">10</option>
        <option name="displayRowNumbers">true</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">log</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Process Crashes and Hangs</title>
        <search>
          <query>sourcetype=ziften* message_type=eventlogrecord eventidentifier=1000 OR eventidentifier=1002 |chart count(eval(eventidentifier=1000)) as "Process Crashes" count(eval(eventidentifier=1002)) as "Process Hangs" count as both by imagefilename|sort -both| fields imagefilename "Process Crashes" "Process Hangs"| head 15</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.chart">bar</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Process Crashes Over Time</title>
        <search>
          <query>sourcetype=ziften* message_type=eventlogrecord |timechart usenull=f count(eval(eventidentifier=1000)) as "Process Crashes" by imagefilename|sort -"Process Crashes"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="charting.chart">area</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="count">10</option>
        <option name="displayRowNumbers">true</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Most Popular Enterprise Processes</title>
        <search>
          <query>sourcetype=ziften* message_type=processinventory|eval basename = replace(imagefilepath, ".*\\\\", "")|stats dc(agentguid) as prev max(basename) as filename by imagefilemd5|sort -prev|fields filename imagefilemd5 prev|head 1000</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Systems With The Most Processes Installed</title>
        <search>
          <query>sourcetype=ziften* message_type=processinventory|stats dc(imagefilemd5) as "Installed Processes" by agentguid|sort -"Installed Processes"|lookup system_info agentguid|eval computername=if(isnull(computername), agentguid, computername)|fields computername "Installed Processes"|head 20</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Systems With The Most Processes Crashes</title>
        <search>
          <query>sourcetype=ziften* message_type=eventlogrecord eventidentifier=1000|stats count as crashes by agentguid|sort -crashes|lookup system_info agentguid|eval computername=if(isnull(computername), agentguid, computername)|fields computername crashes</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="rowNumbers">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Latest Enterprise MD5's</title>
        <search>
          <query>|inputlookup image.csv|eval _time=first_seen |eval basename = replace(sample_path, ".*\\\\", "") |sort -_time|fields _time OS basename companyname fileversion productversion imagefilemd5 sample_path</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
</form>