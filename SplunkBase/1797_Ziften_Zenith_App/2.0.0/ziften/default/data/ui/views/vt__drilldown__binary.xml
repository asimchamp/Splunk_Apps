<form stylesheet="inpage_drilldown_form.css" script="inpage_single_drilldown_form_binary.js">
  <label>VT | Drilldown | Binary</label>
  <description/>
  <fieldset autoRun="true" submitButton="false">
    <input type="text" token="guid" searchWhenChanged="true"/>
    <input type="text" token="md5" searchWhenChanged="true"/>
    <input type="time" searchWhenChanged="true">
      <default>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <chart>
        <title>Binaries Seen With Risky VT Score</title>
        <search><query>sourcetype=ziften:zenith:alert feed="vt" |eval basename = replace(imagefilepath, ".*\\\\", "")|timechart count by basename</query></search>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.chart">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table id="master">
        <title>Binaries With Risky VT Score</title>
        <search><query>sourcetype=ziften:zenith:alert feed="vt"|stats max(score) as "VT Score" first(imagefilepath) as "Sample Path" dc(agentguid) as "Unique MD5 Prev" count as "Gross MD5 Count" min(_time) as "First Seen" max(_time) as "Last Seen" by imagefilemd5|lookup image_info imagefilemd5|sort -"Unique MD5 Prev" |convert ctime("First Seen") ctime("Last Seen")| fields imagefilemd5 "VT Score" "Sample Path"  companyname fileversion productversion "Unique MD5 Prev" "Gross MD5 Count" "First Seen" "Last Seen"</query></search>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Systems With Suspect Binary</title>
        <search><query>sourcetype=ziften:zenith:alert feed="vt" |where imagefilemd5="$md5$" |makemv delim="," details |stats first(imagefilepath) as "Sample Path" max(score) as "VT Score" values(details) as "VT Tags" values(accountname) as "Seen User Associations" by agentguid |lookup system_info agentguid|fields computername "VT Score" "Sample Path" "VT Tags" agentguid</query></search>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="count">10</option>
        <drilldown target="VT Info">
          <link>
            <![CDATA[
  https://www.virustotal.com/latest-scan/$md5$
       ]]>
          </link>
        </drilldown>
      </table>
    </panel>
  </row>
</form>