<form script="custom_layout_width.js">
  <label>System: Search</label>
  <description/>
  <fieldset submitButton="false">
    <input type="time" token="field1" searchWhenChanged="true">
      <label/>
      <default>
        <earliestTime>-24h@h</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
  </fieldset>
  <search><query>sourcetype=ziften:* agentguid=$guid$ |eval basename = replace(imagefilepath, ".*\\\\", "")|stats dc(imagefilepath) as path_count values(imagefilepath) as paths dc(basename) as basename_count values(basename) as basenames dc(fileversion) as fileversion_count values(fileversion) as fileversions dc(productversion) as productversion_count values(productversion) as productversions count(eval(eventtype="autostarts" OR eventtype="autoruns" OR eventtype="services")) as autostart_count count(eval(eventtype="eventlogrecord")) as crash_count count(eval(eventtype="services")) as service_count dc(ports) as port_count values(port) as ports dc(ipaddress) as ip_count values(ipaddress) as ips dc(accountname) as accountname_count values(accountname) as accountnames dc(companyname) as companyname_count values(companyname) as companynames dc(productname) as productname_count values(productname) as productnames dc(internalname) as internalname_count values(internalname) as internalnames dc(originalfilename) as originalfilename_count values(originalfilename) as originalfilenames by imagefilemd5</query></search>
  <row>
    <panel>
      <input type="text" token="system_filter" searchWhenChanged="true">
        <label>Search Filter</label>
        <default>*</default>
      </input>
      <table id="master">
        <title>Systems</title>
        <search><query>sourcetype=ziften:zenith:systeminventory $system_filter$|stats latest(osname) as osname latest(computername) as computername by agentguid|eval osname=replace(osname,"\|.*", "")|eval computername=if(isnull(computername), agentguid, computername)|fields computername osname agentguid</query>
        <earliest>$earliest$</earliest>
        <latest>$latest$</latest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="count">10</option>
        <drilldown>
          <set token="guid">$row.agentguid$</set>
          <set token="form.guid">$row.agentguid$</set>
          <set token="computername">$row.computername$</set>
          <set token="form.computername">$row.computername$</set>
          <unset token="md5"/>
          <unset token="form.md5"/>
        </drilldown>
      </table>
    </panel>
    <panel>
      <chart>
        <title>Operating System Breakout</title>
        <search><query>|inputlookup systems.csv|stats count by osname</query></search>
        <earliestTime>0</earliestTime>
        <latestTime/>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table depends="$guid$">
        <title>System Details $computername$</title>
        <search><query>sourcetype=ziften:* eventtype=systeminventory agentguid=$guid$|head 1|table computername username domain dnshostname ziftenagentversion cpuscore numberofcores numberoflogicalprocessors osname osversion ossuitemask osinstalldate addresswidth totalvisiblememorysize processormanufacturer processorname processordescription computermodel computermanufacturer maxclockspeed baseboardmanufacturer videocontrollername baseboardproduct|transpose|rename column as attribute "row 1" as details</query></search>
        <earliestTime>0</earliestTime>
        <latestTime/>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="count">23</option>
      </table>
    </panel>
    <panel>
      <chart depends="$guid$">
        <title>System Activity</title>
        <search><query>sourcetype=ziften:*   agentguid=$guid$|timechart count by message_type</query></search>
        <earliestTime>$field1.earliest$</earliestTime>
        <latestTime>$field1.latest$</latestTime>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
      <chart depends="$guid$">
        <title>alerts</title>
        <search><query>sourcetype=ziften:zenith:alert agentguid=$guid$|timechart count by message_type</query></search>
        <earliestTime>$field1.earliest$</earliestTime>
        <latestTime>$field1.latest$</latestTime>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <searchPostProcess>stats dc(imagefilemd5) as processes count(eval(service_count &gt; 0)) as services count(eval(autostart_count &gt; 0)) as autostart</searchPostProcess>
        <option name="list.drilldown">full</option>
        <option name="list.wrap">1</option>
        <option name="maxLines">5</option>
        <option name="raw.drilldown">full</option>
        <option name="rowNumbers">false</option>
        <option name="table.drilldown">all</option>
        <option name="table.wrap">1</option>
        <option name="type">list</option>
        <option name="wrap">true</option>
        <option name="dataOverlayMode">none</option>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">line</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table depends="$guid$">
        <title>System Binaries</title>
        <searchPostProcess>eval is_autostart=if(autostart_count &gt; 0, "True", "False")|eval is_service=if(service_count &gt; 0, "True", "False")|eval is_networked=if(ip_count &gt; 0, "True", "False")
          |fields companynames basenames fielversions productversions is_autostart is_service is_networked accountnames paths imagefilemd5| sort companynames</searchPostProcess>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="count">10</option>
        <drilldown>
          <set token="md5">$row.imagefilemd5$</set>
          <set token="form.md5">$row.imagefilemd5$</set>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table depends="$md5$">
        <title>Binary Details</title>
        <search><query>sourcetype=ziften:* eventtype=processinventory agentguid=$guid$ imagefilemd5=$md5$|head 1|table imagefilemd5 imagefilepath companyname fileversion productversion|transpose</query></search>
        <earliestTime>0</earliestTime>
        <latestTime/>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
    <panel>
      <table depends="$md5$">
        <title>autostarts</title>
        <search><query>sourcetype=ziften:* autoruns OR eventtype=autostarts OR eventtype=services agentguid=$guid$ imagefilemd5=$md5$|stats values(name) as servicename values(registrykey) as autorun values(entrypath) as autostart|transpose</query></search>
        <earliestTime>0</earliestTime>
        <latestTime/>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
    <panel>
      <table depends="$md5$">
        <title>ports</title>
        <searchPostProcess>search imagefilemd5=$md5$|table ports</searchPostProcess>
        <earliestTime>0</earliestTime>
        <latestTime/>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
    <panel>
      <table depends="$md5$">
        <title>ipaddresses</title>
        <searchPostProcess>search imagefilemd5=$md5$|table ips</searchPostProcess>
        <earliestTime>0</earliestTime>
        <latestTime/>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
  </row>
</form>