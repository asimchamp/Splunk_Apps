          ]]>
          </link>
        </drilldown>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="numberPrecision">0</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>        
        <option name="drilldown">all</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Most Active Attack Vectors</title>
        <search><query>          
          eventtype=fe product!="HX" OR (eventtype=fe product="HX" severity="IOC Hit") | top limit=10 product | eval percent = round(percent,2) | rename product AS Product, count AS Count, percent AS Percent        
        </query></search>
        <earliest>$earliest$</earliest>
        <latest>$latest$</latest>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Most Active Appliances</title>
        <search><query>          
          eventtype=fe product!="HX" OR (eventtype=fe product="HX" severity="IOC Hit") | top limit=20 dvc_host | eval percent = round(percent,2) | rename dvc_host AS Appliance, count AS Count, percent AS Percent      
        </query></search>
        <earliest>$earliest$</earliest>
        <latest>$latest$</latest>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Events per Attack Vector</title>
        <search><query>          
          eventtype=fe product!="HX" OR (eventtype=fe product="HX" severity="IOC Hit") | timechart count by product      
        </query></search>
        <earliest>$earliest$</earliest>
        <latest>$latest$</latest>
        <option name="charting.chart.stackMode">stacked</option>
      </chart>
    </panel>
  </row>
</form>