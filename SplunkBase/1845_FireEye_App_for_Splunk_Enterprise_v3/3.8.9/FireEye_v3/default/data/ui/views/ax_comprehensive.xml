<form script="inpage_drilldown_form.js" stylesheet="custom.css" version="1.1">
  <label>AX - Comprehensive</label>
  <description>Comprehensive Dashboard for FireEye AX Events</description>
  <!-- Add time range picker -->
  <fieldset autoRun="True" submitButton="false">
    <input type="time" searchWhenChanged="true">
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <!-- Add Wildcard Filter -->
    <input type="text" token="wild" searchWhenChanged="true">
      <label>Search</label>
      <default>*</default>
      <suffix/>
    </input>
    <!-- Used for dropdown - Input field hidden using javascript. <div id = #field3 -->
    <input type="text" token="ID" searchWhenChanged="true"/>
  </fieldset>
  <row>
    <chart>
      <title>Severity</title>
      <search><query>
        eventtype=fe product="MAS" $wild$ | chart count by severity
      </query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.chart.showPercent">true</option>
      <option name="charting.fieldColors">{crit:0xFF1300,majr:0xFFDA00,minr:0x3C04F2}</option>
    </chart>
    <chart>
      <title>Malware</title>
      <search><query>
        eventtype=fe product="MAS" $wild$ |eval signature3=mvappend(signature,signature1,'alert.explanation.malware-detected.malware{}.name') |eval signature3 = mvfilter(NOT match(signature3, "^Binary.*"))| chart count by signature3
      </query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.chart.showPercent">true</option>
    </chart>
    <chart>
      <title>Sensor</title>
      <search><query>
        eventtype=fe product="MAS" $wild$ | chart count by dvc_host
      </query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>File Type</title>
      <search><query>
        eventtype=fe product="MAS" $wild$ |eval file_type3=mvappend(file_type,file_type1,file_type2)| chart count by file_type3
      </query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.chart.showPercent">true</option>
    </chart>
    <chart>
      <title>Top 10 Hashes</title>
      <search><query>
        eventtype=fe product="MAS" $wild$ | top limit=10 file_hash
      </query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">pie</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <drilldown target="_blank">
          <link>/app/FireEye_v3/vtlookup?UrlHash=$row.file_hash$</link>
      </drilldown>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <table id="master">
      <title>Alerts</title>
      <search><query>
        eventtype=fe product="MAS" * |eval file_hash3=mvappend(file_hash,file_hash1,file_hash2,'alert.explanation.malware-detected.malware{}.md5sum')| eval file_type3=mvappend(file_type,file_type1,file_type2) | eval signature3=mvappend(signature,signature1,'alert.explanation.malware-detected.malware{}.name') |eval signature3 = mvfilter(NOT match(signature3, "^Binary.*"))| eval malicious =mvappend('alert.explanation.malware-detected.malware.malicious','alert.explanation.malware-detected.malware{}.malicious') |table Ack, dvc_host, id, file_type3, signature3, severity, file_hash3,malicious, occurred | sort -occurred | rename dvc_host AS Appliance, id AS ID, file_type3 AS FT, signature3 AS Malware, severity AS Severity, file_hash3 AS "Md5sum",malicious As Malicious, occurred AS "Complete (UTC)", locations as "Location"</query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="wrap">true</option>
      <option name="rowNumbers">false</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">cell</option>
    </table>
  </row>
  <row>
    <table id="detail">
      <title>Alert Detail: $ID$</title>
      <search><query>
        eventtype=fe product="MAS" id=$ID$ $wild$ | table _raw, file_hash ext_ref | rename _raw AS "Raw Event", file_hash as UrlHash, ext_ref AS "Event Link"
      </query></search>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
      <option name="wrap">true</option>
      <option name="rowNumbers">false</option>
      <option name="dataOverlayMode">none</option>
      <option name="drilldown">cell</option>
        <drilldown target="_blank">
          <condition field="Raw Event"></condition>
          <condition field="UrlHash"><link target="_blank">/app/$app$/vtlookup?UrlHash=$row.UrlHash$</link></condition>
          <condition field="Event Link">$row.ext_ref$</condition>
        </drilldown>
      <option name="count">10</option>
    </table>
  </row>
</form>