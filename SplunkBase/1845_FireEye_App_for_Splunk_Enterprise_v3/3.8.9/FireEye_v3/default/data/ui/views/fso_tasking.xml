<form stylesheet="custom.css" version="1.1">
  <label>FSO Tasking</label>
  <search><query>| script fsotask __EXECUTE__ action="$action$" src_ip="$src_ip$" dest_host="$dest_host$" md5="$md5$" url="$url$" domain="$domain$" message="$message$"</query></search>
  <description>Interact with FSO</description>
  <fieldset autoRun="false" submitButton="true">
    <input type="dropdown" token="action" searchWhenChanged="false">
      <label>Action</label>
      <default>block</default>
      <choice value="block">Block</choice>
      <choice value="unblock">Unblock</choice>
      <choice value="IntelQuery">Intel Query</choice>
      <choice value="ticket">Ticket</choice>
      <choice value="AddIOC">Add IOC</choice>
      <search><query>
        
      </query></search>
    </input>
    <input type="text" token="src_ip">
      <label>Source IP</label>
      <default>""</default>
    </input>
    <input type="text" token="dest_host">
      <label>Destination Host</label>
      <default>""</default>
    </input>
    <input type="text" token="md5">
      <label>MD5 Hash</label>
      <default>""</default>
    </input>
    <input type="text" token="url">
      <label>URL</label>
      <default>""</default>
    </input>
    <input type="text" token="domain">
      <label>Domain</label>
      <default>""</default>
    </input>
    <input type="text" token="message">
      <label>Message</label>
      <default>""</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Raw Response</title>
        <search base="global">
          <query>| table Key,Value</query>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="count">70</option>
      </table>
    </panel>
  </row>
</form>