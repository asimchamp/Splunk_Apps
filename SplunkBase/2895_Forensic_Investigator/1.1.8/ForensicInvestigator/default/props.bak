

[w32system]
SHOULD_LINEMERGE = false
KV_MODE=xml
TRUNCATE=0
TIME_PREFIX =created\=\"
TIME_FORMAT = %Y-%m-%dT%H:%M:%S
MAX_TIMESTAMP_LOOKAHEAD = 19
LINE_BREAKER = ([\r\n]*)(?=\<SystemInfoItem|\<\/itemList)
TRANSFORMS-xmlClean = xmlbeginNULL,xmlendNULL


