<form stylesheet="custom.css">
  <label>URL History - Analysis</label>
  <!-- Add time range picker -->
  <fieldset submitButton="true">
    <!-- Add Wildcard Filter -->
    <input type="time" token="time1" searchWhenChanged="true">
      <label></label>
      <default>
        <earliest>0</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="text" token="wild" searchWhenChanged="true">
      <label>Wildcard</label>
      <default>*</default>
    </input>
    <input type="text" token="host" searchWhenChanged="true">
      <label>Host</label>
      <default>*</default>
    </input>
    <input type="text" token="UrlHistoryItem.Profile" searchWhenChanged="true">
      <label>Profile</label>
      <default>*</default>
    </input>
    <input type="text" token="UrlHistoryItem.Username" searchWhenChanged="true">
      <label>Username</label>
      <default>*</default>
    </input>
    <input type="text" token="UrlHistoryItem.BrowserName" searchWhenChanged="true">
      <label>Browser Name</label>
      <default>*</default>
    </input>
    <input type="text" token="UrlHistoryItem.BrowserVersion" searchWhenChanged="true">
      <label>Browser Version</label>
      <default>*</default>
    </input>
    <input type="text" token="UrlHistoryItem.URL" searchWhenChanged="true">
      <label>URL</label>
      <default>*</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <search>
          <query>eventtype=mir sourcetype=urlhistory $wild$ host=$host$ UrlHistoryItem.Profile=$UrlHistoryItem.Profile$ UrlHistoryItem.Username=$UrlHistoryItem.Username$ UrlHistoryItem.BrowserName=$UrlHistoryItem.BrowserName$ UrlHistoryItem.BrowserVersion=$UrlHistoryItem.BrowserVersion$ UrlHistoryItem.URL=$UrlHistoryItem.URL$ | table _time, host, UrlHistoryItem.LastVisitDate, UrlHistoryItem.Profile, UrlHistoryItem.Username, UrlHistoryItem.BrowserName, UrlHistoryItem.BrowserVersion, UrlHistoryItem.URL | rename _time AS "Date Pulled", UrlHistoryItem.LastVisitDate AS "Last Visit Date", UrlHistoryItem.Profile AS Profile, UrlHistoryItem.Username AS Username, UrlHistoryItem.BrowserName AS "Browser Name", UrlHistoryItem.BrowserVersion AS "Browser Version", UrlHistoryItem.URL AS URL</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
</form>