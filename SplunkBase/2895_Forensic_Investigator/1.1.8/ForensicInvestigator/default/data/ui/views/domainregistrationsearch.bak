<form stylesheet="custom.css">
  <label>Domain Registration Search</label>
  <searchTemplate>| script domainregistration __EXECUTE__ "$input$"</searchTemplate>
      <description>Enter the Email associated with the domain</description>
  <!-- Add time range picker -->
  <fieldset autoRun="True" submitButton="true">
    <!-- Add Wildcard Filter -->
    <input type="text" token="input" searchWhenChanged="true">
      <label>Email:</label>
      <suffix/>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Result</title>
        <searchPostProcess>table answer | rename answer as Answer</searchPostProcess>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="count">70</option>
      </table>
    </panel>
  </row>
</form>
