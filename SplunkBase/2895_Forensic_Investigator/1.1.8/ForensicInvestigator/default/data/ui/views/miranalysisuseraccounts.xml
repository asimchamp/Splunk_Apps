<form stylesheet="custom.css">
  <label>User Account - Analysis</label>
  <fieldset autoRun="true">
    <input type="time" token="time1" searchWhenChanged="true">
      <label></label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="text" token="wild" searchWhenChanged="true">
      <label>Wildcard</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="hostname" searchWhenChanged="true">
      <label>Host Name</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="securitytype" searchWhenChanged="true">
      <label>Security Type</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="username" searchWhenChanged="true">
      <label>Username</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Raw Data</title>
        <search>
          <query>eventtype=mir sourcetype=w32useraccounts $wild$ host=$hostname$ UserItem.SecurityType=$securitytype$ UserItem.Username=$username$ | table host, UserItem.SecurityType, UserItem.Username, UserItem.fullname, UserItem.lockedout, UserItem.disabled, UserItem.passwordrequired, UserItem.LastLogin, UserItem{@created}, UserItem.grouplist.groupname, UserItem.description</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
</form>