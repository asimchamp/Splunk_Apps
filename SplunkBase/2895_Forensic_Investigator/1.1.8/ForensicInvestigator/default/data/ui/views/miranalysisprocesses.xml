<form stylesheet="custom.css">
  <label>Processes - Analysis</label>
  <fieldset autoRun="true">
    <input type="time" token="time1" searchWhenChanged="true">
      <label></label>
      <default>
        <earliest>0</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="text" token="wild" searchWhenChanged="true">
      <label>Wildcard</label>
      <default>*</default>
    </input>
    <input type="text" token="host" searchWhenChanged="true">
      <label>Host</label>
      <default>*</default>
    </input>
    <input type="text" token="username" searchWhenChanged="true">
      <label>Username</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="pid" searchWhenChanged="true">
      <label>PID</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="ppid" searchWhenChanged="true">
      <label>PPID</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="path" searchWhenChanged="true">
      <label>Path</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="processname" searchWhenChanged="true">
      <label>Process Name</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="arguments" searchWhenChanged="true">
      <label>Arguments</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <label>miranalysisprocesses</label>
  <row>
    <panel>
      <table>
        <title>Raw</title>
        <search>
          <query>eventtype=mir sourcetype=w32processes $wild$ host=$host$ ProcessItem.Username=$username$ ProcessItem.pid=$pid$ ProcessItem.parentpid=$ppid$ ProcessItem.path=$path$ ProcessItem.name=$processname$ ProcessItem.arguments=$arguments$ | table host, ProcessItem.SecurityID, ProcessItem.SecurityType, ProcessItem.Username, ProcessItem.pid, ProcessItem.parentpid, ProcessItem.path, ProcessItem.name, ProcessItem.arguments</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
</form>