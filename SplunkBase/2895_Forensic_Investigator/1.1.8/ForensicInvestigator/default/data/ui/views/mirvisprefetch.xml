<form stylesheet="custom.css">
  <label>Prefetch - Visualization</label>
  <fieldset autoRun="true">
    <input type="time" token="time1" searchWhenChanged="true">
      <label></label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="text" token="wild" searchWhenChanged="true">
      <label>Wildcard</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="host" searchWhenChanged="true">
      <label>Host Name</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Rare ApplicationFileName</title>
        <search>
          <query>eventtype=mir sourcetype=w32prefetch $wild$ host=$host$ | rare limit=0 PrefetchItem.ApplicationFileName</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">false</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Powershell Run</title>
        <search>
          <query>eventtype=mir sourcetype=w32prefetch $wild$ host=$host$ PrefetchItem.ApplicationFileName="*powershell*" | top limit=0 host</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">false</option>
        <option name="drilldown">row</option>
        <option name="dataOverlayMode">none</option>
        <option name="count">10</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>cmd.exe Run</title>
        <search>
          <query>eventtype=mir sourcetype=w32prefetch $wild$ host=$host$ PrefetchItem.ApplicationFileName="cmd.exe" | top limit=0 host</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">false</option>
        <option name="drilldown">row</option>
        <option name="dataOverlayMode">none</option>
        <option name="count">10</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>rundll32.exe Run</title>
        <search>
          <query>eventtype=mir sourcetype=w32prefetch $wild$ host=$host$ PrefetchItem.ApplicationFileName="rundll32.exe" | top limit=0 host</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="drilldown">cell</option>
        <option name="dataOverlayMode">none</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Rare ApplicationFullPath</title>
        <search>
          <query>eventtype=mir sourcetype=w32prefetch $wild$ host=$host$ | rare limit=0 PrefetchItem.ApplicationFullPath</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">false</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
</form>