<form stylesheet="custom.css">
  <label>Prefetch - Analysis</label>
  <fieldset autoRun="true">
    <input type="time" token="time1" searchWhenChanged="true">
      <label></label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="text" token="wild" searchWhenChanged="true">
      <label>Wildcard</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="host" searchWhenChanged="true">
      <label>Host Name</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Events</title>
        <search>
          <query>eventtype=mir sourcetype=w32prefetch $wild$ host=$host$ | table host, PrefetchItem.Created, PrefetchItem.LastRun, PrefetchItem.ApplicationFileName, PrefetchItem.ApplicationFullPath, PrefetchItem.FullPath, PrefetchItem.SizeInBytes, PrefetchItem.PrefetchHash, PrefetchItem.TimesExecuted, PrefetchItem.AccessedFileList.AccessedFile</query>
          <earliest>$time1.earliest$</earliest>
          <latest>$time1.latest$</latest>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
</form>