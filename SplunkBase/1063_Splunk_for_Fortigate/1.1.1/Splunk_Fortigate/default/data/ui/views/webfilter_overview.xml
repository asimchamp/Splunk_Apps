              </param>
            </param>
          </param>
          <param name="intention">
            <param name="name">stringreplace</param>
            <param name="arg">
              <param name="categorization">
                <param name="default"></param>
                <param name="prefix">categorization="</param>
                <param name="suffix">"</param>
                <param name="fillOnEmpty">false</param>
              </param>
            </param>
          </param>
          <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp3">
            <param name="label">URL:</param>
            <param name="field">site</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="site">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="site">
                  <param name="default"></param>
                  <param name="prefix">site="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>
	    <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp3">
            <param name="label"> User :</param>
            <param name="field">user</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="user">
                  <param name="value"></param>
