<view template="dashboard.html" stylesheet="traffic_overview.css">
  <label>Denied Traffic Dashboard</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="navigationHeader" />
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="viewHeader">
    <param name="actionsMenuFilter">dashboard</param>
  </module>
  <module name="GenericHeader" layoutPanel="panel_row1_col1">
    <param name="label">Dened Traffic Dashboard</param>
  </module>
  <module name="StaticContentSample" layoutPanel="panel_row1_col1">
    <param name="text">This dashboard provides visibility into the deny access in your Fortinet firewalls environment.</param>
  </module>
  <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
    <param name="label">Source IP:</param>
    <param name="field">source_ip</param>
    <param name="replacementMap">
      <param name="arg">
        <param name="source_ip">
          <param name="value"></param>
        </param>
      </param>
    </param>
    <param name="intention">
      <param name="name">stringreplace</param>
      <param name="arg">
        <param name="source_ip">
          <param name="default"></param>
          <param name="prefix">source_ip="</param>
          <param name="suffix">"</param>
          <param name="fillOnEmpty">false</param>
        </param>
      </param>
    </param>

 	<module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
            <param name="label">Device name:</param>
            <param name="field">device_name</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="device_name">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="device_name">
                  <param name="default"></param>
                  <param name="prefix">device_name="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>

 	<module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
            <param name="label">VDom:</param>
            <param name="field">vdom</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="vdom">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="vdom">
                  <param name="default"></param>
                  <param name="prefix">vdom="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>

    <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp2">
      <param name="label">Destination IP:</param>
      <param name="field">destination_ip</param>
      <param name="replacementMap">
        <param name="arg">
          <param name="destination_ip">
            <param name="value"></param>
          </param>
        </param>
      </param>
      <param name="intention">
        <param name="name">stringreplace</param>
        <param name="arg">
          <param name="destination_ip">
            <param name="default"></param>
            <param name="prefix">destination_ip="</param>
            <param name="suffix">"</param>
            <param name="fillOnEmpty">false</param>
          </param>
        </param>
      </param>

      <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp2">
        <param name="label">Destination Port:</param>
        <param name="field">destination_port</param>
        <param name="replacementMap">
          <param name="arg">
            <param name="destination_port">
              <param name="value"></param>
            </param>
          </param>
        </param>
        <param name="intention">
          <param name="name">stringreplace</param>
          <param name="arg">
            <param name="destination_port">
              <param name="default"></param>
              <param name="prefix">destination_port="</param>
              <param name="suffix">"</param>
              <param name="fillOnEmpty">false</param>
            </param>
          </param>
        </param>

          <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp3">
            <param name="label">User:</param>
            <param name="field">user</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="user">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="user">
                  <param name="default"></param>
                  <param name="prefix">user="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>

	<module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp3">
            <param name="label">User group:</param>
            <param name="field">group</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="group">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="group">
                  <param name="default"></param>
                  <param name="prefix">group="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>

              <module name="TimeRangePicker" layoutPanel="panel_row2_col1_grp1" autoRun="True">
                <param name="selected">Last 60 minutes</param>
                <param name="searchWhenChanged">True</param>
                <module name="HiddenSearch" layoutPanel="panel_row1_col1">
                  <param name="search">$source_ip$ $device_name$ $vdom$ $destination_ip$ $destination_port$ $user$ $group$ `Fortigate-Traffic-DataCube`</param>
                  <module name="JobProgressIndicator"></module>
                  <module name="ViewRedirectorLink">
                    <param name="viewTarget">flashtimeline</param>
                    <param name="label">View Full Report</param>
                  </module>
                  <module name="HiddenPostProcess" layoutPanel="panel_row4_col1">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Over-Time`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Number of denies over time</param>
                     <param name="chart">line</param>
                        <param name="charting.legend.nullValueMode">connect</param>
                        <param name="charting.legend.showMarkers">true</param>
                        <param name="chart.stackMode">unstacked</param> 
			<param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

                  <module name="HiddenPostProcess" layoutPanel="panel_row5_col1">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Top-Src-IP`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top denies by source IP</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

                  <module name="HiddenPostProcess" layoutPanel="panel_row5_col2">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Top-Src-User`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top denies by source users</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>
		
		<module name="HiddenPostProcess" layoutPanel="panel_row5_col3">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Top-User-Group`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top denies by source users groups</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

                  <module name="HiddenPostProcess" layoutPanel="panel_row6_col1">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Top-Dst-IP`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top denies by destination IP</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

		<module name="HiddenPostProcess" layoutPanel="panel_row6_col2">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Top-Country`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top denies by destination countries</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>


		 <module name="HiddenPostProcess" layoutPanel="panel_row6_col3">
                    <param name="search">`Fortigate-Traffic-DataCube-Deny-Top-Dst-Port`</param>
                    <module name="HiddenChartFormatter">
                      <param name="chartTitle">Top denies by destination ports</param>
                      <param name="chart">pie</param>
                      <param name="legend.placement">bottom</param>
                      <module name="FlashChart">
                        <param name="width">100%</param>
                        <param name="height">250px</param>
                        <module name="ConvertToDrilldownSearch">
                          <module name="ViewRedirector">
                            <param name="viewTarget">flashtimeline</param>
                          </module>
                        </module>
                      </module>
                    </module>
                  </module>

		 <module name="HiddenPostProcess" layoutPanel="panel_row7_col1">
                  <param name="search">`Fortigate-Traffic-DataCube-Deny-Device`</param>
                  <module name="HiddenChartFormatter">
                    <param name="chartTitle">Top denies by Fortigate and vdom devices</param>
                    <param name="chart">pie</param>
                    <param name="legend.placement">bottom</param>
                    <module name="FlashChart">
                      <param name="width">100%</param>
                      <param name="height">250px</param>
                      <module name="ConvertToDrilldownSearch">
                        <module name="ViewRedirector">
                          <param name="viewTarget">flashtimeline</param>
                        </module>
                      </module>
                    </module>
                  </module>
                </module>

		<module name="HiddenPostProcess" layoutPanel="panel_row7_col2">
                  <param name="search">`Fortigate-Traffic-DataCube-Deny-Device-Policy`</param>
                  <module name="HiddenChartFormatter">
                    <param name="chartTitle">Top denies by Fortigate, VDom and policy ID</param>
                    <param name="chart">pie</param>
                    <param name="legend.placement">bottom</param>
                    <module name="FlashChart">
                      <param name="width">100%</param>
                      <param name="height">250px</param>
                      <module name="ConvertToDrilldownSearch">
                        <module name="ViewRedirector">
                          <param name="viewTarget">flashtimeline</param>
                        </module>
                      </module>
                    </module>
                  </module>
                </module>

		 <module name="HiddenPostProcess" layoutPanel="panel_row8_col1">
                <param name="search">`Fortigate-Traffic-DataCube-Deny-Activity`</param>
                <module name="Paginator">
                  <param name="count">100</param>
                  <param name="entityName">results</param>
                  <module name="SimpleResultsTable">
                    <param name="drilldown">row</param>
                    <module name="ConvertToDrilldownSearch">
                      <module name="ViewRedirector">
                        <param name="viewTarget">flashtimeline</param>
                      </module>
                    </module>
                  </module>
                </module>
              </module>
		</module>
		</module>	
                </module>
              </module>
            </module>
          </module>
        </module>
      </module>
    </module>
</view>
