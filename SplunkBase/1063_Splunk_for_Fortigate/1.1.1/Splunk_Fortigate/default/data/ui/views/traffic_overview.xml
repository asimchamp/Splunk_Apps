<view template="dashboard.html" stylesheet="traffic_overview.css">
  <label>Traffic Dashboard</label>
  <module name="AccountBar" layoutPanel="appHeader" />
  <module name="AppBar" layoutPanel="navigationHeader" />
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="viewHeader">
    <param name="actionsMenuFilter">dashboard</param>
  </module>
  <module name="GenericHeader" layoutPanel="panel_row1_col1">
    <param name="label">Traffic Dashboard</param>
  </module>
  <module name="StaticContentSample" layoutPanel="panel_row1_col1">
    <param name="text">This dashboard provides visibility into the traffic data in your Fortinet firewalls environment.</param>
  </module>
  <module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
    <param name="label">Source IP:</param>
    <param name="field">source_ip</param>
    <param name="replacementMap">
      <param name="arg">
        <param name="source_ip">
          <param name="value"></param>
        </param>
      </param>
    </param>
    <param name="intention">
      <param name="name">stringreplace</param>
      <param name="arg">
        <param name="source_ip">
          <param name="default"></param>
          <param name="prefix">source_ip="</param>
          <param name="suffix">"</param>
          <param name="fillOnEmpty">false</param>
        </param>
      </param>
    </param>

 	<module name="ExtendedFieldSearch" layoutPanel="panel_row2_col1_grp1">
            <param name="label">Device name:</param>
            <param name="field">device_name</param>
            <param name="replacementMap">
              <param name="arg">
                <param name="device_name">
                  <param name="value"></param>
                </param>
              </param>
            </param>
            <param name="intention">
              <param name="name">stringreplace</param>
              <param name="arg">
                <param name="device_name">
                  <param name="default"></param>
                  <param name="prefix">device_name="</param>
                  <param name="suffix">"</param>
                  <param name="fillOnEmpty">false</param>
                </param>
              </param>
            </param>

