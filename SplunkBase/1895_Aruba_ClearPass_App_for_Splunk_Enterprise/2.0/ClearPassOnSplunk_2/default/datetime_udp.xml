<datetime>

<define name="_datetimecppm" extract="year, month, day, hour, minute, second, subsecond, zone">
<!-- Target TimeFormat: 2014-01-09 22:33:35.123+00:00 -->
<text><![CDATA[(\d\d\d\d)-(\d\d)-(\d\d)\s(\d{2}):(\d{2}):(\d{2}).(\d{3})(\S+)]]></text>
</define>


<timePatterns>
<use name="_datetimecppm"/>
</timePatterns>
<datePatterns>
<use name="_datetimecppm"/>
</datePatterns>

</datetime>
